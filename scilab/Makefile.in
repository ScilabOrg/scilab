clean-reports:
	rm -f reports/xml/TEST-org.scilab.*.xml

# For each module, build and call Java unitary tests
# If fails, stops the call
check-java: clean-reports
	@echo "-- Launch all Junit (Java) unitary tests"; \
	MASK="$(top_srcdir)/modules/*/tests/java/"; \
	MODULES=`ls -d $$MASK`; \
	for M in $$MODULES; do \
		BASE=`echo $$M|sed -e "s|tests/java/$$||g"`; \
		$(ANT) -buildfile $$BASE/build.xml test || exit $$?; \
	done
	rm -f reports/xml/TEST-org.scilab.*.xml

codecoverage-java: clean-reports
	@echo "Check if --enable-debug-java has been set"; \
	grep "build.debug=on" $(top_srcdir)/scilab.properties > /dev/null || exit 10; \
	echo "-- Launch all Junit (Java) unitary tests + code coverage"; \
	MASK="$(top_srcdir)/modules/*/tests/java/"; \
	MODULES=`ls -d $$MASK`; \
	for M in $$MODULES; do \
		BASE=`echo $$M|sed -e "s|tests/java/$$||g"`; \
		$(ANT) -buildfile $$BASE/build.xml codecoverage || exit $$?; \
	done
	rm -f reports/xml/TEST-org.scilab.*.xml

# The user wants to compile the localization files
# Please note that ALL_LINGUAS is defined in configure.ac
@GENERATE_LOCALIZATION_FILES_TRUE@localization:
@GENERATE_LOCALIZATION_FILES_TRUE@	@echo "-- Build localization (.mo from .po) --"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	MASK="modules/*/locales/"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	LC="LC_MESSAGES"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	MSGFMT_OPTS="$MSGFMT_OPTS --check"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	EXTENSION="*.po"; \
@GENERATE_LOCALIZATION_FILES_TRUE@	rm modules/*/locales/*~ 2> /dev/null; \
@GENERATE_LOCALIZATION_FILES_TRUE@	for LOCALE in $(ALL_LINGUAS); do \
@GENERATE_LOCALIZATION_FILES_TRUE@		echo "        -- Building for $$LOCALE --"; \
@GENERATE_LOCALIZATION_FILES_TRUE@		FILES=`ls $$MASK/$$LOCALE$$EXTENSION`; \
@GENERATE_LOCALIZATION_FILES_TRUE@		PATHTO=$(top_builddir)/locale/$$LOCALE/$$LC/; \
@GENERATE_LOCALIZATION_FILES_TRUE@		if test ! -d $$PATHTO; then \
@GENERATE_LOCALIZATION_FILES_TRUE@			echo "        Creating $$PATHTO"; \
@GENERATE_LOCALIZATION_FILES_TRUE@			mkdir -p $$PATHTO; \
@GENERATE_LOCALIZATION_FILES_TRUE@		fi; \
@GENERATE_LOCALIZATION_FILES_TRUE@		POFILE=$$PATHTO/$(LANG_DOMAIN).po; \
@GENERATE_LOCALIZATION_FILES_TRUE@		$(MSGCAT) --use-first -o $$POFILE $$FILES; \
@GENERATE_LOCALIZATION_FILES_TRUE@		if test "$(MACOSX)" = "1"; then sed -i -e '/charset=UTF-8/d' $$POFILE; fi; \
@GENERATE_LOCALIZATION_FILES_TRUE@		$(MSGFMT) $(MSGFMT_OPTS) --statistics -o $$PATHTO/$(LANG_DOMAIN).mo $$POFILE; \
@GENERATE_LOCALIZATION_FILES_TRUE@	done;

@GENERATE_LOCALIZATION_FILES_TRUE@localization-status:
@GENERATE_LOCALIZATION_FILES_TRUE@	@for LOCALE in $(ALL_LINGUAS_DOC); do \
@GENERATE_LOCALIZATION_FILES_TRUE@		if test "$$LOCALE" != "en_US"; then \
@GENERATE_LOCALIZATION_FILES_TRUE@			PATHTO=$(top_builddir)/locale-status/$$LOCALE/; \
@GENERATE_LOCALIZATION_FILES_TRUE@			if test ! -d $$PATHTO; then \
@GENERATE_LOCALIZATION_FILES_TRUE@				echo "        Creating $$PATHTO"; \
@GENERATE_LOCALIZATION_FILES_TRUE@				mkdir -p $$PATHTO; \
@GENERATE_LOCALIZATION_FILES_TRUE@			fi; \
@GENERATE_LOCALIZATION_FILES_TRUE@			echo "Building status for $$LOCALE"; \
@GENERATE_LOCALIZATION_FILES_TRUE@			echo "SCI=$(top_srcdir) php tools/localization/revcheck.php $$LOCALE > $$PATHTO/index.html"; \
@GENERATE_LOCALIZATION_FILES_TRUE@			SCI=$(top_srcdir) php tools/localization/revcheck.php $$LOCALE > $$PATHTO/index.html; \
@GENERATE_LOCALIZATION_FILES_TRUE@		fi; \
@GENERATE_LOCALIZATION_FILES_TRUE@	done;

@GENERATE_LOCALIZATION_FILES_TRUE@install-data-local:
@GENERATE_LOCALIZATION_FILES_TRUE@	@$(mkinstalldirs) $(DESTDIR)$(localedir); \
@GENERATE_LOCALIZATION_FILES_TRUE@	echo $(mkinstalldirs) $(DESTDIR)$(localedir); \
@GENERATE_LOCALIZATION_FILES_TRUE@	for lang in $(ALL_LINGUAS); do \
@GENERATE_LOCALIZATION_FILES_TRUE@		LANG_PATH_DIST=$(DESTDIR)$(localedir)/$$lang/LC_MESSAGES/; \
@GENERATE_LOCALIZATION_FILES_TRUE@		echo $(mkinstalldirs) $$LANG_PATH_DIST; \
@GENERATE_LOCALIZATION_FILES_TRUE@		$(mkinstalldirs) $$LANG_PATH_DIST; \
@GENERATE_LOCALIZATION_FILES_TRUE@		echo $(INSTALL_DATA) $(top_builddir)/locale/$$lang/LC_MESSAGES/$(LANG_DOMAIN).mo $$LANG_PATH_DIST/; \
@GENERATE_LOCALIZATION_FILES_TRUE@		$(INSTALL_DATA) $(top_builddir)/locale/$$lang/LC_MESSAGES/$(LANG_DOMAIN).mo $$LANG_PATH_DIST/; \
@GENERATE_LOCALIZATION_FILES_TRUE@	done

