install-infofilesDATA: $(infofiles_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(infofilesdir)" || $(MKDIR_P) "$(DESTDIR)$(infofilesdir)"
	@list='$(infofiles_DATA)'; test -n "$(infofilesdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(infofilesdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(infofilesdir)" || exit $$?; \
	done

uninstall-infofilesDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(infofiles_DATA)'; test -n "$(infofilesdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(infofilesdir)'; $(am__uninstall_files_from_dir)
