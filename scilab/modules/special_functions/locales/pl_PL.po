#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: Nieprawidłowy typ argumentu wejściowego nr %d lub nr %d: argument nie "
"może być liczbą zespoloną.\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s: Nieprawidłowa wartość argumentów wejściowych: muszą być >%d.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Nieprawidłowy typ pierwszego argumentu wejściowego.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Nieprawidłowy typ dla argumentu wejściowego nr %d.\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Tylko jeden z arg1 i arg2 może być wektorem.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s: Niewłaściwy typ argumentu wejściowego nr %d: oczekiwana macierz wartości "
"typu real.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: Nieprawidłowa wartość argumentu wejściowego nr %d: oczekiwana macierz "
"elementów z zakresu (%d,%d).\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr ""
"%s: Przepełnienie lub niedomiar dla liczb o rozszerzonym zakresie "
"dokładności\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: Numer błędu %d\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr ""
"%s: Nieprawidłowy typ argumentu wejściowego nr %d: oczekiwany typ real.\n"
