#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: вектором може бути лише один з двох параметрів.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: помилковий тип першого вхідного параметра.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d або №%d: не можна використовувати "
"комплексні вхідні параметри.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: помилковий тип вхідного параметра №%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"#%s: помилковий тип вхідного параметра №%d: слід використовувати матрицю "
"дійсних значень.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: слід використовувати матрицю "
"з елементами у (%d,%d).\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s: помилкове значення вхідних параметрів: значення мають бути > %d.\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: номер помилки %d\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr "%s: переповнення або втрата значимості у розширеному діапазоні чисел\n"





