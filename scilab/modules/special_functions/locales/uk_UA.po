#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s: помилкова розмірність вхідного параметра №%d.\n"

#, c-format
msgid "%s: arguments #%d and #%d have incompatible dimensions.\n"
msgstr "%s: параметри №%d і №%d мають несумісні розмірності.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати матрицю "
"дійсних значень.\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr ""
"%s: помилкове значення вхідних параметрів: значення мають бути > %d.\n"

#, c-format
msgid "%s: Can not read input argument #%d.\n"
msgstr "%s: не вдалося прочитати вхідний параметр №%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A real expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип дійсних "
"значень.\n"

#, c-format
msgid "%s: Wrong type for argument #%d: A matrix expected.\n"
msgstr "%s: помилковий тип параметра №%d: мало бути використано матрицю.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Integer or vector of integers "
"expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути використано ціле число "
"або вектор з цілих чисел.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer >= %d expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути використано ціле число "
">= %d.\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: вектором може бути лише один з двох параметрів.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in [%d,%d] "
"expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: мало бути використано матрицю "
"з елементами з діапазону [%d,%d].\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr ""
"%s: переповнення або втрата значимості у розширеному діапазоні чисел\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: номер помилки %d\n"

#, c-format
msgid "%s: Wrong type for argument #%d: Real or complex matrix expected.\n"
msgstr ""
"%s: помилковий тип параметра №%d: слід використовувати матрицю з дійсних або "
"комплексних значень.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s: помилкова кількість вхідних параметрів: мало бути %d.\n"

#, c-format
msgid "%s: Wrong value for argument #%d: 0, 1 or 2 expected.\n"
msgstr ""
"%s: помилкове значення параметра №%d: мало бути використано 0, 1 або 2.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип дійсних "
"значень.\n"
