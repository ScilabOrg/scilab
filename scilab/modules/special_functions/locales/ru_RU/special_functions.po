#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr "%s: Неверный тип аргумента #%d: Ожидалось вещественное число.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: Неверный тип аргумента #%d или #%d: Комплексные аргументы недопустимы.\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s: Неверны значения аргументов: Должны быть > %d.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Неверный тип первого аргумента.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Неверный тип аргумента #%d.\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Только один из arg1 и arg2 может быть вектором.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s: Неверный тип аргумента #%d: Ожидалась матрица вещественных чисел.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: Неверное значение аргумента #%d: Ожидалсь матрица с элементами в "
"(%d,%d).\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr ""
"%s: переполнение или потеря значимости в расширенном диапазоне чисел\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: ошибка номер %d\n"
