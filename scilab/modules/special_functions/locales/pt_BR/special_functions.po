#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s: Apenas um de arg1 e arg2 deve ser um vetor.\n"

#, c-format
msgid "%s: Wrong type for first input argument.\n"
msgstr "%s: Valor errado para o primeiro argumento de entrada.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d or #%d: No complex input argument "
"expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d ou #%d: Não era esperado "
"nenhum argumento de entrada complexo.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Tipo errado para o argumento de entrada #%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Experava-se um número "
"real.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Esperava-se uma matriz de "
"números reais.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in (%d,%d) "
"expected.\n"
msgstr ""
"%s: Valor errado para o argumento de entrada #%d: Esperava-se matriz com "
"elementos em (%d,%d).\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s: Valor errado para os argumentos de entrada: Deve ser > %d.\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s: erro número %d\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr "%s: overflow ou underflow de um número de intervalo estendido\n"
