#, c-format
msgid " %s.mo cannot be compiled\n"
msgstr " %s.mo nie może być skompilowany\n"

#, c-format
msgid " %s.mo compilation forced\n"
msgstr " %s.mo przymusowa kompilacja\n"

#, c-format
msgid " %s.mo must be recompiled\n"
msgstr " %s.mo muszą być re-kompilowane\n"

#, c-format
msgid "%s: An error occurred during the compilation of the Modelica block."
msgstr "%s: Wystąpił błąd podczas kompilacji bloku Modelica."

#, c-format
msgid "%s: Cannot find any MO files in the following location: %s.\n"
msgstr ""
"%s: Nie można znaleźć żadnych plików MO w następującej lokalizacji: %s.\n"

#, c-format
msgid "%s: Diagram does not compile in pass %d.\n"
msgstr ""

#, c-format
msgid "%s: Error : the following command line failed to execute: %s.\n"
msgstr "%s: Błąd : nie wykonano następujących poleceń wiersza: %s.\n"

#, c-format
msgid "%s: Error: A scifunc block has not been defined."
msgstr "%s: Błąd: blok scifunc nie został zdefiniowany."

#, c-format
msgid "%s: Error: Modelica compiler (MODELICAC) is unavailable."
msgstr "%s: Błąd: kompilator modelica (MODELICAC) jest niedostępny."

#, c-format
msgid "%s: Input ports are not numbered properly.\n"
msgstr "%s: Porty wejściowe nie są właściwie ponumerowane.\n"

#, c-format
msgid "%s: Output ports are not numbered properly.\n"
msgstr "%s: Porty wyjściowe nie są numerowane odpowiednio.\n"

#, c-format
msgid "%s: Wrong number of input arguments. Must be between %d and %d.\n"
msgstr ""
"%s: Nieprawidłowa liczba argumentów wejściowych. Musi być między %d i %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s: Nieprawidłowa liczba argumentów wejściowych: oczekiwano %d.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr "%s: Nieprawidłowa liczba argumentów wyjściowych: oczekiwano %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single integer expected.\n"
msgstr ""
"%s: Nieprawidłowy rozmiar argumentu wejściowego #%d: oczekiwana pojedyncza "
"liczba całkowita.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single string expected.\n"
msgstr ""
"%s: Nieprawidłowy rozmiar argumentu wejściowego #%d: Oczekiwano pojedynczego "
"łańcuchu znaków.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d.\n"
msgstr "%s: Nieprawidłowy rozmiar argumentów wejściowych #%d i #%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr ""
"%s: Nieprawidłowy typ parametru wejściowego #%d: Oczekiwana wartości "
"całkowita.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: Nieprawidłowy typ argumentu wejściowego #%d: oczekiwany jest ciąg "
"znaków.\n"

msgid "&?"
msgstr "&?"

msgid "&Edit"
msgstr "&Edycja"

msgid "&File"
msgstr "&Plik"

msgid "&Tools"
msgstr "&Narzędzia"

msgid "''scicos'' is no more available, please use ''xcos'' instead.\n"
msgstr "''scicos'' nie jest już dostępny, proszę użyć ''xcos'' zamiast.\n"

msgid "A compatible C compiler required."
msgstr "Wymagany zgodny kompilator C."

msgid "Only one block can be selected in current window for this operation."
msgstr ""
"Tylko jeden blok może zostać wybrany w bieżącym oknie do tej operacji."

#, c-format
msgid "Problem while building library %s\n"
msgstr "Problem przy budowie biblioteki %s\n"

msgid "Sorry compiling problem"
msgstr "Przepraszamy problem kompilacji"

msgid "The current icon depends on block parameter\n"
msgstr "Bieżąca ikona zależy od parametrów bloku\n"
