#, c-format
msgid " %s.mo cannot be compiled\n"
msgstr " %s.mo ne peut pas être compilée\n"

#, c-format
msgid " %s.mo compilation forced\n"
msgstr " Compilation de %s.mo forcée\n"

#, c-format
msgid " %s.mo must be recompiled\n"
msgstr " %s.mo doit être recompilée\n"

#, c-format
msgid "%s: An error occurred during the compilation of the Modelica block."
msgstr ""
"%s: Une erreur s'est produite pendant la compilation du bloc Modelica."

#, c-format
msgid "%s: Cannot find any MO files in the following location: %s.\n"
msgstr ""
"%s: Impossible de trouver les fichiers MO dans le répertoire suivant: %s.\n"

#, c-format
msgid "%s: Diagram does not compile in pass %d.\n"
msgstr "%s: Le diagramme ne compile pas à l'étape %d.\n"

#, c-format
msgid "%s: Error : the following command line failed to execute: %s.\n"
msgstr "%s: Erreur : la commande suivante n'a pas pu s'exécuter: %s.\n"

#, c-format
msgid "%s: Error: A scifunc block has not been defined."
msgstr "%s: Erreur: Un bloc scifunc n'a pas été défini."

#, c-format
msgid "%s: Error: Modelica compiler (MODELICAC) is unavailable."
msgstr ""
"%s: Erreur: Le compilateur Modelica (modelicac) n'est pas disponible."

#, c-format
msgid "%s: Input ports are not numbered properly.\n"
msgstr "%s: Les ports d'entrée ne sont pas numérotés correctement.\n"

#, c-format
msgid "%s: Output ports are not numbered properly.\n"
msgstr "%s: Les ports de sortie ne sont pas numérotés correctement.\n"

#, c-format
msgid "%s: Wrong number of input arguments. Must be between %d and %d.\n"
msgstr "%s: Mauvais nombre d'arguments d'entrée. Doit être entre %d et %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s : Mauvais nombre d'arguments d'entrée : %d attendu.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr "%s : Mauvais nombre d'arguments de sortie : %d attendus.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single integer expected.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée n° %d: Un seul entier "
"attendu.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single string expected.\n"
msgstr ""
"%s : Mauvaise dimension pour l'argument d'entrée n° %d : Une simple chaîne "
"de caractères attendue.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d.\n"
msgstr "%s : Mauvaise dimension pour les arguments d'entrée n°%d et n°%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr "%s : Mauvais type pour l'argument d'entrée n°%d : Entier attendu.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s : Mauvais type pour l'argument n° %d : Chaîne de caractères attendue.\n"

msgid "&?"
msgstr "&?"

msgid "&Edit"
msgstr "&Edition"

msgid "&File"
msgstr "&Fichier"

msgid "&Tools"
msgstr "&Outils"

msgid "''scicos'' is no more available, please use ''xcos'' instead.\n"
msgstr "''scicos'' n'est plus accessible, veuillez utiliser ''xcos''.\n"

msgid "A compatible C compiler required."
msgstr "Un compilateur C est nécessaire."

msgid "Only one block can be selected in current window for this operation."
msgstr ""
"Un seul bloc peut être sélectionné dans la fenêtre actuelle pour cette "
"opération."

#, c-format
msgid "Problem while building library %s\n"
msgstr "Un problème est arrivé en construisant la bibliothèque %s\n"

msgid "Sorry compiling problem"
msgstr "Désolé pour ce probleme de compilation"

msgid "The current icon depends on block parameter\n"
msgstr "L'icône courant dépend des paramètres du bloc\n"
