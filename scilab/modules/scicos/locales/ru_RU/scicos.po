#, c-format
msgid " %s.mo cannot be compiled\n"
msgstr " Не удалось скомпилировать %s.mo\n"

#, c-format
msgid " %s.mo compilation forced\n"
msgstr " Вынужденная компиляция %s.mo\n"

#, c-format
msgid " %s.mo must be recompiled\n"
msgstr " %s.mo должен быть перекомпилирован\n"

#, c-format
msgid "%s: An error occurred during the compilation of the Modelica block."
msgstr "%s: Произошла ошибка при компиляции блока Modelica."

#, c-format
msgid "%s: Cannot find any MO files in the following location: %s.\n"
msgstr "%s: Не удалось найти какие-либо MO файлы по следующему адресу: %s.\n"

#, c-format
msgid "%s: Diagram does not compile in pass %d.\n"
msgstr "%s: Диаграмма не скомпилирована в %d проход(а).\n"

#, c-format
msgid "%s: Error : the following command line failed to execute: %s.\n"
msgstr "%s: Ошибка: не удалось выполнить следующую команду: %s.\n"

#, c-format
msgid "%s: Error: A scifunc block has not been defined."
msgstr "%s: Ошибка: блок scifunc не определён."

#, c-format
msgid "%s: Error: Modelica compiler (MODELICAC) is unavailable."
msgstr "%s: Ошибка: компилятор Modelica (MODELICAC) недоступен."

#, c-format
msgid "%s: Input ports are not numbered properly.\n"
msgstr "%s: Порты ввода не пронумерованы правильно.\n"

#, c-format
msgid "%s: Output ports are not numbered properly.\n"
msgstr "%s: Порты вывода не пронумерованы правильно.\n"

#, c-format
msgid "%s: Wrong number of input arguments. Must be between %d and %d.\n"
msgstr "%s: Неверный количество входных параметров: ожидалось от %d до %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s: Неверное количество входных параметров: ожидалось %d.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr "%s: Неверное количество выходных параметров: ожидалось %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single integer expected.\n"
msgstr ""
"%s: Неверный размер входного параметра №%d: ожидалась одиночная строка.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single string expected.\n"
msgstr ""
"%s: Неверный размер входного параметра №%d: ожидалась одиночная строка.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d.\n"
msgstr "%s: Неверный размер входных параметров №%d и №%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr "%s: Неверный тип входного параметра №%d: ожидалось целое число.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr "%s: Неверный тип входного параметра №%d: ожидалась строка.\n"

msgid "&?"
msgstr "&Справка"

msgid "&Edit"
msgstr "&Правка"

msgid "&File"
msgstr "&Файл"

msgid "&Tools"
msgstr "&Инструменты"

msgid "''scicos'' is no more available, please use ''xcos'' instead.\n"
msgstr "«scicos» более недоступен, используйте «xcos».\n"

msgid "A compatible C compiler required."
msgstr "Требуется компилятор С или совместимый"

msgid "Only one block can be selected in current window for this operation."
msgstr "Только один блок может быть выбран в текущем окне для этой операции."

#, c-format
msgid "Problem while building library %s\n"
msgstr "Проблема при сборке библиотеки %s\n"

msgid "Sorry compiling problem"
msgstr "Ошибка компиляции"

msgid "The current icon depends on block parameter\n"
msgstr "Текущий значок зависит от параметра блока\n"
