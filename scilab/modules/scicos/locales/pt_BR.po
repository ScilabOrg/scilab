#, c-format
msgid " %s.mo cannot be compiled\n"
msgstr " %s.mo não pode ser compilado\n"

#, c-format
msgid " %s.mo compilation forced\n"
msgstr " Compilação de %s.mo forçada\n"

#, c-format
msgid " %s.mo must be recompiled\n"
msgstr " %s.mo deve ser recompilado\n"

#, c-format
msgid "%s: An error occurred during the compilation of the Modelica block."
msgstr "%s: Ocorreu um erro durante a compilação do bloco Modelica."

#, c-format
msgid "%s: Cannot find any MO files in the following location: %s.\n"
msgstr "%s: Não se consegue achar arquivos MO na seguinte localização: %s.\n"

#, c-format
msgid "%s: Diagram does not compile in pass %d.\n"
msgstr "%s: O diagrama não compila na passagem %d.\n"

#, c-format
msgid "%s: Error : the following command line failed to execute: %s.\n"
msgstr "%s: Erro: a seguinte linha de comando não pôde ser executada: %s.\n"

#, c-format
msgid "%s: Error: A scifunc block has not been defined."
msgstr "%s: Erro: Um bloco scifunc não foi definido."

#, c-format
msgid "%s: Error: Modelica compiler (MODELICAC) is unavailable."
msgstr "%s: Erro: O compilador Modelica (MODELICAC) não está disponível."

#, c-format
msgid "%s: Input ports are not numbered properly.\n"
msgstr "%s: As portas de entrada não estão numeradas apropriadamente.\n"

#, c-format
msgid "%s: Output ports are not numbered properly.\n"
msgstr "%s: As portas de saída não estão numeradas apropriadamente.\n"

#, c-format
msgid "%s: Wrong number of input arguments. Must be between %d and %d.\n"
msgstr ""
"%s: Quantidade incorreta de argumentos de entrada: deve estar entre %d e "
"%d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s: Quantidade incorreta de argumentos de entrada: esperava-se %d.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr "%s: Quantidade incorreta de argumentos de saída: esperava-se %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single integer expected.\n"
msgstr ""
"%s: Tamanho incorreto para o argumento de entrada #%d: esperava-se um único "
"número inteiro.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Single string expected.\n"
msgstr ""
"%s: Tamanho incorreto para o argumento de entrada #%d: esperava-se um único "
"texto.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d.\n"
msgstr "%s: Tamanho incorreto para os argumentos de entrada #%d e #%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um número "
"inteiro.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um texto.\n"

msgid "&?"
msgstr "&?"

msgid "&Edit"
msgstr "&Editar"

msgid "&File"
msgstr "&Arquivo"

msgid "&Tools"
msgstr "&Ferramentas"

msgid "''scicos'' is no more available, please use ''xcos'' instead.\n"
msgstr ""
"''scicos'' não está mais disponível. Por favor, use ''xcos'' no lugar.\n"

msgid "A compatible C compiler required."
msgstr "É necessário um compilador C compatível."

msgid "Only one block can be selected in current window for this operation."
msgstr ""
"Apenas um bloco pode ser selecionado na janela atual para esta operação."

#, c-format
msgid "Problem while building library %s\n"
msgstr "Problema na construção da biblioteca %s\n"

msgid "Sorry compiling problem"
msgstr "Desculpe, problema de compilação"

msgid "The current icon depends on block parameter\n"
msgstr "O ícone atual depende de um parâmetro do bloco.\n"
