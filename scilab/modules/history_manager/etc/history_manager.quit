// ====================================================================
// This file contains command to be executed at the end
// of a scilab session for history_manager module
// ====================================================================

// save history file
if (sciargs()<>'-nouserstartup') then
		// save history file
    savehistory();
    // disable history manager
    historymanager('off');
end

// delete help chapter
del_help_chapter('history_manager',%T);
