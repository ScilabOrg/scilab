msgstr ""

msgid "   minimize  f(X1,...,XM)  "
msgstr ""

msgid "subject to the LME constraints: "
msgstr ""

msgid "   Gi(X1,...,XM)=0,  i=1,2,...,p,"
msgstr ""

msgid "and the LMI constraints:  "
msgstr ""

msgid "   Hj(X1,...,XM)>=0,  j=1,2,...,q."
msgstr ""

msgid "where"
msgstr ""

