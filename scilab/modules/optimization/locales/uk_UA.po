msgid ""
"\n"
"    primal obj.  dual obj.  dual. gap \n"
msgstr ""
"\n"
"    основн. ціл. двоїст. ціл. двоїст. різн. \n"

#, c-format
msgid "%s: Accuracy insufficient to satisfy convergence criterion.\n"
msgstr "%s: недостатня точність для задоволення критерію збіжності.\n"

#, c-format
msgid "%s: Argument 3: wrong number of columns %d expected\n"
msgstr "%s: параметр 3: помилкова кількість стовпчиків: мало бути %d\n"

#, c-format
msgid "%s: Cannot allocate more memory.\n"
msgstr "%s: не вдалося виділити більше пам’яті.\n"

#, c-format
msgid "%s: Length of working array is too short.\n"
msgstr "%s: довжина робочого масиву є занадто малою.\n"

#, c-format
msgid "%s: Q is not symmetric positive definite.\n"
msgstr "%s: Q не є симетричною додатно визначеною.\n"

#, c-format
msgid "%s: The constraints are inconsistent.\n"
msgstr "%s: суперечливі обмеження.\n"

#, c-format
msgid "%s: The minimization problem has no solution.\n"
msgstr "%s: задача з мінімізації не має розв’язків.\n"

#, c-format
msgid "%s: Too many iterations (more than %d).\n"
msgstr "%s: забагато ітерацій (більше за %d).\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: number of columns %d expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: кількість стовпчиків має "
"дорівнювати %d.\n"

#, c-format
msgid "Error in dgels, info = %d.\n"
msgstr "Помилка у dgels, info = %d.\n"

#, c-format
msgid "Error in dspev, info = %d.\n"
msgstr "Помилка у dspev, info = %d.\n"

#, c-format
msgid "Error in dspgv, info = %d.\n"
msgstr "Помилка у dspgv, info = %d.\n"

#, c-format
msgid "Error in dspst, info = %d.\n"
msgstr "Помилка у dspst, info = %d.\n"

#, c-format
msgid "Error in dtrcon, info = %d.\n"
msgstr "Помилка у dtrcon, info = %d.\n"

msgid "F(x)*Z has a negative eigenvalue.\n"
msgstr "F(x)*Z має від’ємне власне значення.\n"

msgid "L must be at least one.\n"
msgstr "L має дорівнювати принаймні одному.\n"

msgid "Matrices Fi, i=1,...,m are linearly dependent.\n"
msgstr "Матриці Fi, i=1,...,m є лінійно залежними.\n"

msgid ""
"The matrices F_i, i=1,...,m are linearly dependent (or the initial points "
"are very badly conditioned).\n"
msgstr ""
"Матриці F_i, i=1,..,m є лінійно залежними (або погана обумовленість "
"початкових точок).\n"

#, c-format
msgid "Work space is too small.  Need at least %d*sizeof(double).\n"
msgstr "Замало пам’яті для роботи. Потрібно принаймні %d*sizeof(double).\n"

msgid "Z0 does not satisfy equality conditions for dual feasibility.\n"
msgstr "Z0 не задовольняє умови рівності двоїстої задачі.\n"

msgid "Z0 is not positive definite.\n"
msgstr "Z0 не є додатно визначеною.\n"

#, c-format
msgid "blck_szs[%d] must be at least one.\n"
msgstr "blck_szs[%d] має дорівнювати принаймні одному.\n"

msgid "m must be at least one.\n"
msgstr "m має дорівнювати принаймні одному.\n"

msgid "nu must be at least 1.0.\n"
msgstr "nu має бути не меншим за 1.0.\n"

msgid "x0 is not strictly primal feasible.\n"
msgstr "x0 не є строгим розв’язком основної задачі.\n"
