// <-- Short Description -->
// Captions plots legend are in reverse order
f = gcf();
plot([1,2], [1,10], "b");
h1 = gce();
h1 = h1.children(1);
plot([1,2], [1,20], "r");
h2 = gce();
h2 = h2.children(1);
c = captions([h1, h2], ["1"; "2"]);
assert_checkequal(c.text, ["1"; "2"]);
delete(f);
