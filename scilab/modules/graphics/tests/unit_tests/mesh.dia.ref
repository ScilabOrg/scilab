// <-- TEST WITH GRAPHIC -->
// test fac3d object
// Check each default property
[X, Y] = meshgrid(-1:.1:1, -1:.1:1);
Z = X.^2 - Y.^2;
mesh(X,Y,Z);
e = gce();
assert_checkequal(e.type, "Fac3d")
