// <-- TEST WITH GRAPHIC -->
// test scf gcf and delete behaviour
scf(3);
scf(2);
scf(1);
f = gcf();
assert_checkequal(f.figure_id, 1);
delete(f);
f = gcf();
assert_checkequal(f.figure_id, 3);
delete(f);
f = gcf();
assert_checkequal(f.figure_id, 2);
delete(f);
f = gcf();
assert_checkequal(f.figure_id, 0);
delete(f);
