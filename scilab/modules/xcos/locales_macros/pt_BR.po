#, c-format
msgid "%s is not a valid name, please change the title of the diagram."
msgstr "%s não é um nome válido. Altere o título do diagrama."

#, c-format
msgid "%s: All paths defined by %s variable must exist.\n"
msgstr "%s: Todos os caminhos definidos pela variável %s devem existir.\n"

#, c-format
msgid "%s: File ''%s'' does not exist.\n"
msgstr "%s: O arquivo ''%s'' não existe.\n"

#, c-format
msgid "%s: File ''%s'' is not a valid palette file.\n"
msgstr "%s: O arquivo ''%s'' não é um arquivo válido de paleta.\n"

#, c-format
msgid "%s: Found ''%s'' instead of a block.\n"
msgstr "%s: Encontrou ''%s'' em vez de um bloco.\n"

#, c-format
msgid ""
"%s: The directory %s cannot been created, please check if you have write "
"access on this directory.\n"
msgstr ""
"%s: O diretório %s não pode ser criado. Por favor, confira se você tem "
"permissão de escrita neste diretório.\n"

#, c-format
msgid "%s: Unable to export %s to %s.\n"
msgstr "%s: Incapaz de exportar %s para %s.\n"

#, c-format
