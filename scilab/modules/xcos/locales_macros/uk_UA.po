#, c-format
msgid "%s is not a valid name, please change the title of the diagram."
msgstr "%s є некоректною назвою, будь ласка, змініть назву діаграми."

#, c-format
msgid "%s: All paths defined by %s variable must exist.\n"
msgstr "%s: всі шляхи, визначені змінною %s, мають існувати.\n"

#, c-format
msgid "%s: File ''%s'' does not exist.\n"
msgstr "%s: файла з назвою «%s» не існує.\n"

#, c-format
msgid "%s: File ''%s'' is not a valid palette file.\n"
msgstr "%s: файл «%s» не є коректним файлом палітри.\n"

#, c-format
msgid "%s: Found ''%s'' instead of a block.\n"
msgstr "%s: замість блоку виявлено «%s».\n"

#, c-format
msgid ""
"%s: The directory %s cannot been created, please check if you have write "
"access on this directory.\n"
msgstr ""
"%s: не вдалося створити каталог %s, будь ласка, перевірте, чи є у вас права "
"доступу на запис до відповідного каталогу.\n"

#, c-format
msgid "%s: Unable to export %s to %s.\n"
msgstr "%s: не вдалося експортувати %s до %s.\n"

#, c-format
msgid "%s: Unable to export ''XX'' to %s"
msgstr "%s: не вдалося експортувати «XX» до %s"

#, c-format
msgid "%s: Unable to export the palette to hdf5.\n"
msgstr "%s: не вдалося експортувати палітру до hdf5.\n"

#, c-format
msgid "%s: Unable to generate the image %s.\n"
msgstr "%s: не вдалося створити зображення %s.\n"

#, c-format
msgid "%s: Unable to generate the palette icon : %s already exists.\n"
msgstr "%s: не вдалося створити піктограму палітри: %s вже існує.\n"

#, c-format
msgid "%s: Unable to import data from %s"
msgstr "%s: не вдалося імпортувати дані з %s"

#, c-format
msgid "%s: Unable to import xcos file %s.\n"
msgstr "%s: не вдалося імпортувати файл xcos %s.\n"

#, c-format
msgid "%s: Unable to load block from %s: hdf5 file expected.\n"
msgstr ""
"%s: не вдалося завантажити блок з %s: мало бути використано файл hdf5.\n"

#, c-format
msgid "%s: Unable to load block from %s: no `scs_m'' variable found.\n"
msgstr "%s: не вдалося завантажити блок з %s: не виявлено змінної «scs_m».\n"

