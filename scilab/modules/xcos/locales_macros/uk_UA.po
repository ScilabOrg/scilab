#, c-format
msgid "%s is not a valid name, please change the title of the diagram."
msgstr "%s є некоректною назвою, будь ласка, змініть назву діаграми."

#, c-format
msgid "%s: All paths defined by %s variable must exist.\n"
msgstr "%s: всі шляхи, визначені змінною %s, мають існувати.\n"

#, c-format
msgid "%s: File ''%s'' does not exist.\n"
msgstr "%s: файла з назвою «%s» не існує.\n"

#, c-format
msgid "%s: File ''%s'' is not a valid palette file.\n"
msgstr "%s: файл «%s» не є коректним файлом палітри.\n"

#, c-format
msgid "%s: Found ''%s'' instead of a block.\n"
msgstr "%s: замість блоку виявлено «%s».\n"

#, c-format
msgid ""
"%s: The directory %s cannot been created, please check if you have write "
"access on this directory.\n"
msgstr ""
"%s: не вдалося створити каталог %s, будь ласка, перевірте, чи є у вас права "
"доступу на запис до відповідного каталогу.\n"

#, c-format
msgid "%s: Unable to export %s to %s.\n"
msgstr "%s: не вдалося експортувати %s до %s.\n"

#, c-format
msgid "%s: Unable to export ''XX'' to %s"
msgstr "%s: не вдалося експортувати «XX» до %s"

#, c-format
msgid "%s: Unable to export the palette to hdf5.\n"
msgstr "%s: не вдалося експортувати палітру до hdf5.\n"

#, c-format
msgid "%s: Unable to generate the image %s.\n"
msgstr "%s: не вдалося створити зображення %s.\n"

#, c-format
msgid "%s: Unable to generate the palette icon : %s already exists.\n"
msgstr "%s: не вдалося створити піктограму палітри: %s вже існує.\n"

#, c-format
msgid "%s: Unable to import data from %s"
msgstr "%s: не вдалося імпортувати дані з %s"

#, c-format
msgid "%s: Unable to import xcos file %s.\n"
msgstr "%s: не вдалося імпортувати файл xcos %s.\n"

#, c-format
msgid "%s: Unable to load block from %s: hdf5 file expected.\n"
msgstr ""
"%s: не вдалося завантажити блок з %s: мало бути використано файл hdf5.\n"

#, c-format
msgid "%s: Unable to load block from %s: no `scs_m'' variable found.\n"
msgstr "%s: не вдалося завантажити блок з %s: не виявлено змінної «scs_m».\n"

#, c-format
msgid "%s: Wrong input argument %s: svg, gif or jpg expected.\n"
msgstr ""
"%s: помилковий вхідний параметр %s: мало бути вказано svg, gif або jpg.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s: помилкова кількість вхідних параметрів: мало бути %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати від %d до "
"%d.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr ""
"%s: помилкова кількість вихідних параметрів: слід використовувати %d.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d to %d expected.\n"
msgstr ""
"%s: помилкова кількість вихідних параметрів: слід використовувати значення "
"від %d до %d.\n"

#, c-format
msgid "%s: Wrong type for %s variable: A string array expected.\n"
msgstr ""
"%s: помилковий тип змінної %s: слід використовувати масив з рядків.\n"

#, c-format
msgid "%s: Wrong type for argument %d: String or diagram structure expected"
msgstr ""
"%s: помилковий тип вхідного параметра %d: слід використовувати тип «string» "
"або тип структури діаграми."

#, c-format
msgid "%s: Wrong type for argument %d: diagram structure expected"
msgstr ""
"%s: помилковий тип вхідного параметра %d: слід використовувати тип структури "
"діаграми."

#, c-format
msgid "%s: Wrong type for input argument %s: Block type expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати блоковий "
"тип.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: File not found.\n"
msgstr "%s: помилковий тип вхідного параметра %s: файл не знайдено.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: boolean expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати булівське "
"значення.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: diagram type expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати тип "
"діаграми.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument %s: directory path string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати рядок шляху "
"до каталогу.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: full path string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати рядок повного "
"шляху.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument %s: function as string or Block type or "
"full path string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати функцію, що "
"виводить значення у типах string або Block, або повний рядок шляху.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: handle type expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати тип елемента "
"керування.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: palette type expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати тип палітри.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument %s: palette type or path expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати тип палітри "
"або шлях до файла.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: path string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати рядок шляху "
"до файла.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати рядок.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: string or struct expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати рядок або "
"структуру (struct).\n"

#, c-format
msgid "%s: Wrong type for input argument %s: string type expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати рядковий "
"тип.\n"

#, c-format
msgid "%s: Wrong type for input argument %s: string vector expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра %s: слід використовувати вектор "
"рядків.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument %s: A valid file format (png, jpg, gif) "
"expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра %s: мало бути вказано коректний "
"формат файла (png, jpg, gif).\n"

#, c-format
msgid "%s: Wrong value for input argument %s: An existing file expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра %s: мало бути вказано існуючий "
"файл.\n"

#, c-format
msgid ""
"%s: the block %s is no more available, please update the diagram with a "
"compatible one."
msgstr ""
"%s: блок %s недоступний, будь ласка, додайте замість нього якийсь сумісний "
"блок."

