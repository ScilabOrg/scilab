// When opening diagram by syntax xcos(scs_m) some properties were corrupted
loadXcosLibs();
assert_checktrue(importXcosDiagram("SCI/modules/xcos/demos/Simple_Demo.xcos"));
solver = scs_m.props.tol(6);
xcos(scs_m);
cpr = xcos_compile(scs_m);
new_solver = scs_m.props.tol(6);
assert_checkequal(solver, new_solver);
