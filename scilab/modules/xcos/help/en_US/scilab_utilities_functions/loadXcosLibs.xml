    <refsynopsisdiv>
        <title>Calling Sequence</title>
        <synopsis>loadXcosLibs()</synopsis>
    </refsynopsisdiv>
    <refsection id="Module_loadXcosLibs">
        <title>Module</title>
        <itemizedlist>
