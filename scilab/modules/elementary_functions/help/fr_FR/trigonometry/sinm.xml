          xmlns:db="http://docbook.org/ns/docbook">
  <info>
    <pubdate>$LastChangedDate$</pubdate>
  </info>

  <refnamediv>
    <refname>sinm</refname>

    <refpurpose>sinus matriciel</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Séquence d'appel</title>

    <synopsis>t=sinm(x)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Paramètres</title>

    <variablelist>
      <varlistentry>
        <term>x</term>

        <listitem>
          <para>matrice réelle ou complexe</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>

  <refsection>
    <title>Description</title>

    <para><literal>sinm(x)</literal> est le sinus matriciel de la matrice
    x.</para>
  </refsection>

  <refsection>
    <title>Exemples</title>

    <programlisting role="example"><![CDATA[ 
A=[1,2;2,4];
sinm(A)+0.5*%i*(expm(%i*A)-expm(-%i*A))
 ]]></programlisting>
  </refsection>

  <refsection role="see also">
<title>Voir aussi</title>

    <simplelist type="inline">
      <member><link linkend="sin">sin</link></member>

      <member><link linkend="asinm">asinm</link></member>
    </simplelist>
  </refsection>
</refentry>
