// <-- Short Description -->
// isreal failed with strings (and all other unsupported types).
// OK
isreal([1 2]);
isreal(1+0*%i);
isreal(1+0*%i,0);
isreal(1+%s);
isreal(sprand(3,3,0.1));
// KO
refMsg = msprintf(_("Wrong type for input argument #%d: Real or complex matrix expected.\n"), 1);
assert_checkerror("isreal("""")", refMsg);
assert_checkerror("isreal(%t)", refMsg);
assert_checkerror("isreal(list())", refMsg);
assert_checkerror("isreal(int8(17))", refMsg);
