#, c-format
msgid "%s mlist type not yet implemented."
msgstr "Le type %s mlist n''est pas encore implémenté."

#, c-format
msgid "%s not yet implemented."
msgstr "%s n''est pas encore implémenté."

#, c-format
msgid "%s: Cannot open file %s.\n"
msgstr "%s : Impossible d''ouvrir le fichier %s.\n"

#, c-format
msgid "%s: Could not open file ''%s''.\n"
msgstr "%s : Impossible d''ouvrir le fichier %s.\n"

#, c-format
msgid ""
"%s: No variable read in file ''%s''. Check if your file is not corrupted.\n"
msgstr ""
"%s : Impossible de lire le fichier ''%s''. Veuillez vérifier qu''il n''est "
"pas corrompu.\n"

#, c-format
msgid "%s: This feature has not been implemented: %s."
msgstr "%s : Cette fonctionnalité n''a pas été implémentée : %s."

#, c-format
msgid "%s: Wrong type for input argument #%d: Matrix of strings expected.\n"
msgstr ""
"%s : Type erroné de l''argument d''entrée n°%d : Une matrice de chaîne de "
"caractères attendue.\n"

msgid "All inputs must be character strings."
msgstr "Toutes les entrées doivent être des chaînes de caractères."

msgid "Attempt to write an unsupported data type to an ASCII file."
msgstr ""
"Tentative d''écriture d''un type de données non supporté vers un fichier "
"ASCII."

msgid "Error while writing MI."
msgstr "Erreur à l''écriture du MI."

#, c-format
msgid "Found Datatype=%d, expecting %d."
msgstr "Type de données trouvé : %d, Attendu : %d."

msgid "Invalid level 5 binary MAT-file!."
msgstr "Fichier binaire MAT niveau 5 invalide !"

msgid "MATLAB 5.0 MAT-file, Generated by Scilab"
msgstr "MAT-file MATLAB 5.0, Généré par Scilab"

#, c-format
msgid "Name conflict: it is not possible to save variable with name %s."
msgstr ""
"Conflit de nom : il n''est pas possible d'enregistrer la variable sous le "
"nom %s."

#, c-format
msgid "Not implemented DataType: %d."
msgstr "Type de données non implémenté : %d."

#, c-format
msgid "Option %s not implemented: IGNORED."
msgstr "Option %s non implementée : IGNORÉ."

msgid "Option -v7 added."
msgstr "Option -v7 ajoutée."

msgid "Scilab string matrix saved as Matlab Cell."
msgstr ""
"La matrice de chaînes de caractères Scilab a été enregistrée en tant que "
"Matlab Cell."

msgid "Unknown Class."
msgstr "Classe inconnue."

#, c-format
msgid "Unknown integer type: %s."
msgstr "Type d''entier inconnu : %s."

#, c-format
msgid "Variable %s can not be saved in ASCII file: IGNORED."
msgstr "Impossible d'enregistrer la variable %s au format ASCII : IGNORÉ."

#, c-format
msgid "Variable %s can not be saved in level 4 MAT-file: IGNORED."
msgstr ""
"Impossible d'enregistrer la variable %s dans MAT-file niveau 4 : IGNORÉ."

#, c-format
msgid "Version %d MAT-file not implemented."
msgstr "MAT-file version %d non implémenté."

#, c-format
msgid "savematfile: could not save variable named %s.\n"
msgstr "savematfile : Impossible d'enregistrer la variable nommée %s.\n"
