#, c-format
msgid "%s: 2D array of strings saving is not implemented.\n"
msgstr ""
"%s : La sauvegarde de tableaux 2D de chaînes de caractères n'est pas "
"implémentée.\n"

#, c-format
msgid ""
"%s: Column array of strings with different lengths saving is not "
"implemented.\n"
msgstr ""
"%s : La sauvegarde de tableaux de colonnes de chaînes de caractères de "
"différentes longueurs n'est pas implémentée.\n"

#, c-format
msgid "%s: Could not rewind the file %s.\n"
msgstr "%s : Le fichier %s n'a pu être rechargé.\n"

#, c-format
msgid "%s: Invalid file identifier.\n"
msgstr "%s : Identifieur fichier invalide.\n"

#, c-format
msgid "%s: Mlists of type %s can not be written to Matlab binary files.\n"
msgstr ""
"%s : Les Mlistes de type %s n'ont pu être transformées en fichiers binaires "
"Matlab.\n"

#, c-format
msgid "%s: N-D arrays of chars not implemented.\n"
msgstr "%s : Les tableaux de caractères N-D ne sont pas implémentés.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s : Plus de mémoire disponible.\n"

#, c-format
msgid "%s: Row array of strings saving is not implemented.\n"
msgstr ""
"%s : La sauvegarde de tableaux de lignes de chaînes de caractères de "
"différentes longueurs n'est pas implémentée.\n"

#, c-format
msgid "%s: Wrong size for first input argument: Single double expected.\n"
msgstr ""
"%s : Taille incorrecte du premier argument d'entrée : Un seul double "
"attendu.\n"

#, c-format
msgid "%s: Wrong size for first input argument: Single string expected.\n"
msgstr ""
"%s : Taille incorrecte du premier argument : Une seule chaîne de caractères "
"attendue.\n"

#, c-format
msgid "%s: Wrong size for fourth input argument: Single boolean expected.\n"
msgstr ""
"%s : Taille incorrecte du quatrième argument d'entrée : Un seul booléen "
"attendu.\n"

#, c-format
msgid "%s: Wrong size for second input argument: Single string expected.\n"
msgstr ""
"%s : Taille incorrecte du deuxième argument d'entrée : Une seule chaîne de "
"caractères attendue.\n"

#, c-format
msgid "%s: Wrong type for first input argument: Double expected.\n"
msgstr "%s : Type incorrect du premier argument d'entrée : Double attendu.\n"

#, c-format
msgid "%s: Wrong type for first input argument: Double matrix expected.\n"
msgstr ""
"%s : Type incorrect du premier argument d'entrée : Matrice de Doubles "
"attendue.\n"

#, c-format
msgid "%s: Wrong type for first input argument: Integer matrix expected.\n"
msgstr ""
"%s : Type incorrect du premier argument d'entrée : Matrice d'Entiers "
"attendue.\n"

#, c-format
msgid "%s: Wrong type for first input argument: Mlist expected.\n"
msgstr "%s : Type incorrect du premier argument d'entrée : Mliste attendue.\n"

#, c-format
msgid "%s: Wrong type for first input argument: Single string expected.\n"
msgstr ""
"%s : Type incorrect du premier argument d'entrée : Une seule chaîne de "
"caractères attendue.\n"

#, c-format
msgid "%s: Wrong type for first input argument: Sparse matrix expected.\n"
msgstr ""
"%s : Type incorrect du premier argument d'entrée : Matrice creuse attendue.\n"

#, c-format
msgid "%s: Wrong type for first input argument: String matrix expected.\n"
msgstr ""
"%s : Type incorrect du premier argument d'entrée : Matrice de chaînes de "
"caractères attendue.\n"

#, c-format
msgid "%s: Wrong type for fourth input argument: Single boolean expected.\n"
msgstr ""
"%s : Type incorrect du quatrième argument d'entrée : Un seul booléen "
"attendu.\n"

#, c-format
msgid "%s: Wrong type for second input argument: Single string expected.\n"
msgstr ""
"%s : Type incorrect du deuxième argument d'entrée : Une seule chaîne de "
"caractères attendue.\n"

#, c-format
msgid "%s: Wrong value for second input argument: 'r' or 'w' expected.\n"
msgstr ""
"%s : Valeur incorrecte du deuxième argument d'entrée : 'r' ou 'w' attendu.\n"

