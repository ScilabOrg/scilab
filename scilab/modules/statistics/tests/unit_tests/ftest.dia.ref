// Run with test_run('statistics', 'ftest', ['no_check_error_output']);
prec = 1.e-5;
samples = [46 55 54;
         53 54 50; 
         49 58 51;
         50 61 51;
         46 52 49]
 samples  =
 
    46.    55.    54.  
    53.    54.    50.  
    49.    58.    51.  
    50.    61.    51.  
    46.    52.    49.  
[f, p] = ftest(samples)
 p  =
 
    0.0055319  
 f  =
 
    8.2672065  
expected_p = 0.0055319;
if norm(p-expected_p) > prec then bugmes();quit;end
