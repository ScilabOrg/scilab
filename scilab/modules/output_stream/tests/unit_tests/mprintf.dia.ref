// format '%f'
// =============================================================================
mprintf("==>%f<==\n\n",-35);
==>-35.000000<==

mprintf("==>%f<==\n\n",35.55);
==>35.550000<==

mprintf("==>%f<==\n\n",0.00433);
==>0.004330<==

mprintf("==>%f<==\n\n",0.0000000345456);
==>0.000000<==

mprintf("==>%f<==\n\n",1112423453);
==>1112423453.000000<==

mprintf("==>%15f<==\n\n",-35);
==>     -35.000000<==

mprintf("==>%15f<==\n\n",0.00433);
==>       0.004330<==

mprintf("==>%15f<==\n\n",0.0000000345456);
==>       0.000000<==

mprintf("==>%15f<==\n\n",1112423453);
==>1112423453.000000<==

mprintf("==>%.1f<==\n\n" ,-35);
==>-35.0<==

mprintf("==>%.0f<==\n\n" ,-35);
==>-35<==

mprintf("==>%#.0f<==\n\n",-35);
==>-35.<==

mprintf("==>%.1f<==\n\n" ,0.00433);
==>0.0<==

mprintf("==>%.15f<==\n\n",0.0000000345456);
==>0.000000034545600<==

mprintf("==>%.1f<==\n\n" ,11124234534545);
==>11124234534545.0<==

// format '%g'
