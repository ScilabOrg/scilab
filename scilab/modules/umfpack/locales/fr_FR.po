msgstr "%s : Une erreur est apparue : %s : %s\n"

#, c-format
msgid "%s: No more memory (n=%d, nnz=%d).\n"
msgstr "%s: Plus de mémoire (n=%d, nnz=%d).\n"

#, c-format
msgid ""
"%s: No more memory : increase stacksize %d supplementary words needed.\n"
msgstr ""
"%s: Plus de mémoire disponible: augmentez stacksize %d mots supplémentaires "
"sont requis.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s : Plus de mémoire disponible.\n"

#, c-format
msgid "%s: Wrong input argument #%d: '%s' or '%s' expected.\n"
msgstr "%s: Mauvais argument d'entrée n° %d: '%s' ou '%s' attendu.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s : Mauvaise dimension pour l'argument d'entrée n°%d.\n"

