msgstr "    Factorisation CC^T: La matrice n'est pas définie positive.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s : Une erreur s'est produite : %s\n"

#, c-format
msgid "%s: An error occurred: %s.\n"
msgstr "%s : Une erreur est apparue : %s.\n"

#, c-format
