msgid "Scilab graphic module disabled -nogui or -nwni mode.\n"
msgstr ""
"Il modulo grafico di Scilab ha disabilitato la modalità -nogui o -nwni.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: An integer or a handle expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era atteso un intero o "
"un gestore.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d ha una dimensione sbagliata: era atteso uno "
"scalare.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A graphic handle expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d ha una dimensione sbagliata: era atteso un "
"gestore grafico.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era attesa una stringa "
"di caratteri singoli.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' or '%s' expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d ha un valore sbagliato: era atteso '%s' o "
"'%s'.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d ha una dimensione sbagliata: era attesa una "
"stringa di un solo carattere.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A real between 0 and 1 expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era atteso un numero "
"reale fra 0 e 1.\n"

#, c-format
msgid "%s: %s\n"
msgstr "%s: %s\n"

msgid "Scilab Graphic export module not installed.\n"
msgstr "Il modulo di esportazione Scilab Graphic non è installato.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: '%s', '%s', '%s', '%s', '%s' or '%s' "
"expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era atteso "
"'%s','%s','%s','%s','%s' o '%s'.\n"

#, c-format
msgid "%s: %s must be called before any action.\n"
msgstr "%s: %s deve essere chiamato prima di ogni operazione.\n"

#, c-format
msgid ""
"%s: Wrong number of input arguments: Function has no input argument with "
"'%s' or '%s' drivers.\n"
msgstr ""
"%s: Il numero degli argomenti in ingresso è sbagliato: Function non ha "
"argomenti in ingresso con i driver '%s' o '%s'.\n"

#, c-format
msgid ""
"%s: Wrong number of input argument: String expected with '%s', '%s', '%s' or "
"'%s' drivers.\n"
msgstr ""
"%s: Il numero degli argomenti in ingresso: era attesa una stringa con i "
"driver '%s','%s','%s' o '%s'.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era attesa una "
"stringa.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr ""
"%s: Il numero degli argomenti in ingresso è sbagliato: era atteso da %d a "
"%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr ""
"%s: L'argomento in ingresso #%d è di tipo sbagliato: era atteso un intero.\n"

#, c-format
msgid "%s: EMF format only available under Microsoft Windows OS.\n"
msgstr ""
"%s: Il formato EMF è disponibile solo se si usa un SO Microsoft Windows.\n"

#, c-format
msgid "%s: Unable to find pstoedit.\n"
msgstr "%s: Impossibile trovare pstoedit.\n"

#, c-format
msgid "%s: Unable to create export file, permission denied.\n"
msgstr ""
"%s: Non è possibile creare un file di esportazione,non si dispone del "
"permesso necessario.\n"

#, c-format
msgid "%s: Unable to execute pstoedit.\n"
msgstr "%s: Non è possibile eseguire pstoedit.\n"

msgid "Please install Ghostscript 32 bits to export an .FIG file."
msgstr "Installare Ghostscript a 32 bit per esportare un file .FIG."

msgid "Scilab error"
msgstr "Errore di Scilab"

msgid "error"
msgstr "errore"

#, c-format
msgid ""
"%s: Unable to locate pstoedit. Please install pstoedit or add it to the PATH "
"to be able to use this feature.\n"
msgstr ""
"%s: Non è possibile trovare pstoedit. Si prega di installare pstoedit o di "
"aggiungerlo in PATH per poterlo usare in futuro.\n"
