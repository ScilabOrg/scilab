msgid "%s: Unable to create export file, permission denied.\n"
msgstr "%s: No es pot crear el fitxer d'exportació, permís denegat.\n"

#, c-format
msgid "%s: Unable to execute pstoedit.\n"
msgstr "%s: No es pot executar el pstoedit.\n"

#, c-format
msgid "%s: Unable to find pstoedit.\n"
msgstr ""

#, c-format
msgid ""
"%s: Unable to locate pstoedit. Please install pstoedit or add it to the PATH "
"to be able to use this feature.\n"
msgstr ""
"%s: No s'ha pogut trobar el pstoedit. Instal·leu el pstoedit o afegiu-lo al "
"PATH per poder utilitzar aquesta característica.\n"

#, c-format
msgid ""
"%s: Wrong number of input argument: String expected with ''%s'', ''%s'', "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: El nombre d'arguments d'entrada és incorrecte: Ha de ser una cadena de "
"text amb els controladors ''%s'', ''%s'', ''%s'' o ''%s''.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr ""
"%s: Nombre incorrecte d'arguments d'entrada: Hi ha d'haver entre %d i %d.\n"

#, c-format
msgid ""
"%s: Wrong number of input arguments: Function has no input argument with "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: El nombre d'arguments d'entrada és incorrecte: La funció no té cap "
"argument d'entrada amb els controladors ''%s'' o ''%s''.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A graphic handle expected.\n"
msgstr ""

#, c-format
msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
