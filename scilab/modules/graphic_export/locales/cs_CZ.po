msgid "Scilab graphic module disabled -nogui or -nwni mode.\n"
msgstr "Grafický modul Scilabu je vypnut, režim -nogui nebo -nwni.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: An integer or a handle expected.\n"
msgstr ""
"%s: Špatný typ vstupního argumentu #%d: Očekáváno celé číslo nebo obslužná "
"rutina.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
msgstr "%s: Špatná velikost vstupního argumentu #%d: Očekáván skalár.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A graphic handle expected.\n"
msgstr ""
"%s: Špatná velikost vstupního argumentu #%d: Očekávána grafická obslužná "
"rutina.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: Chybný typ vstupního argumentu #%d: Očekáván řetězec s jedním znakem.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' or '%s' expected.\n"
msgstr ""
"%s: Špatná hodnota vstupního argumentu #%d: Očekáváno \"%s\", nebo \"%s\".\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: Špatná velikost vstupního argumentu #%d: Očekáván řetězec s jedním "
"znakem.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A real between 0 and 1 expected.\n"
msgstr ""
"%s: Špatný typ vstupního argumentu #%d: Očekáváno realné číslo mezi 0 a 1.\n"

#, c-format
msgid "%s: %s\n"
msgstr "%s: %s\n"

msgid "Scilab Graphic export module not installed.\n"
msgstr "Grafický modul exportu Scilab není nainstalován.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: '%s', '%s', '%s', '%s', '%s' or '%s' "
"expected.\n"
msgstr ""
"%s: Špatná hodnota vstupního argumentu #%d: očekáváno '%s', '%s', '%s', "
"'%s', '%s' nebo '%s'.\n"

#, c-format
msgid "%s: %s must be called before any action.\n"
msgstr "%s: %s musí být zavolán před jakoukoli činností.\n"

#, c-format
msgid ""
"%s: Wrong number of input arguments: Function has no input argument with "
"'%s' or '%s' drivers.\n"
msgstr ""
"%s: Špatný počet vstupních argumentů: Funkce nemá vstupní argumenty s "
"ovladači '%s' nebo '%s'.\n"

#, c-format
msgid ""
"%s: Wrong number of input argument: String expected with '%s', '%s', '%s' or "
"'%s' drivers.\n"
msgstr ""
"%s: Špatný počet vstupních argumentů: Očekáván řetězec s '%s', '%s', '%s' "
"nebo ovladači '%s'.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr "%s: Špatný typ vstupního argumentu #%d: Očekáván řetězec.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr "%s: Špatný počet vstupních argumentů: očekáváno %d až %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr "%s: Špatný typ vstupního argumentu #%d: Očekáváno celé číslo.\n"

#, c-format
msgid "%s: EMF format only available under Microsoft Windows OS.\n"
msgstr "%s: Formát EMF je dostupný pouze v OS Microsoft Windows.\n"

#, c-format
msgid "%s: Unable to find pstoedit.\n"
msgstr "%s: Nelze nalézt pstoedit.\n"

#, c-format
msgid "%s: Unable to create export file, permission denied.\n"
msgstr "%s: Nelze vytvořit exportní soubor, přístup odepřen.\n"

#, c-format
msgid "%s: Unable to execute pstoedit.\n"
msgstr "%s: Nelze spustit pstoedit.\n"

msgid "Please install Ghostscript 32 bits to export an .FIG file."
msgstr "Nainstalujte si, prosím, Ghostscript 32 bitů pro export souboru .FIG"

msgid "Scilab error"
msgstr "Chyba Scilab"

msgid "error"
msgstr "chyba"

#, c-format
msgid ""
"%s: Unable to locate pstoedit. Please install pstoedit or add it to the PATH "
"to be able to use this feature.\n"
msgstr ""
"%s: Nelze nalézt pstoedit. Nainstalujte si, prosím, pstoedit a přidejte ho "
"do PATH, abyste mohli tuto funkci použít.\n"
