msgid "Scilab graphic module disabled -nogui or -nwni mode.\n"
msgstr "O módulo gráfico do Scilab desabilitou o modo -nogui ou o -nwni.\n"

#, c-format
msgid "%s: Can not read input argument #%d.\n"
msgstr "%s: Não foi possível ler o argumento de entrada #%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A string expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um texto.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Memória esgotada.\n"

#, c-format
msgid "%s: Invalid driver: %s.\n"
msgstr "%s: driver inválido: %s.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Ocorreu um erro: %s.\n"

#, c-format
msgid "%s: Invalid path: %s.\n"
msgstr "%s: caminho inválido: %s.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: An integer or a handle expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um inteiro "
"ou um manipulador.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
msgstr ""
"%s: Tamanho incorreto para o argumento de entrada #%d: esperava-se um "
"escalar.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A graphic handle expected.\n"
msgstr ""
"%s: Tamanho incorreto para o argumento de entrada #%d: esperava-se um "
"manipulador gráfico.\n"

#, c-format
msgid "%s: Input argument #%d must be a valid handle.\n"
msgstr ""

#, c-format
msgid "%s: Wrong type for input argument #%d: A ''%s'' handle expected.\n"
msgstr ""

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um único "
"texto.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' or '%s' expected.\n"
msgstr ""
"%s: Valor incorreto para o argumento de entrada #%d: esperava-se '%s' ou "
"'%s'.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Single character string expected.\n"
msgstr ""
"%s: Tamanho incorreto para o argumento de entrada #%d: esperava-se um texto "
"de um único caractere.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A real between 0 and 1 expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um número "
"real entre 0 e 1.\n"

#, c-format
msgid "%s: %s\n"
msgstr "%s: %s\n"

msgid "xinit must be called before xend."
msgstr "xinit tem que ser chamada antes de xend."

msgid "Unable to create export file, permission denied."
msgstr "Impossível criar arquivo de exportação, permissão negada."

msgid "Unable to create export file, invalid file."
msgstr "Impossível criar arquivo de exportação, arquivo inválido."

msgid ""
"Unable to create export file, not enough memory. Decreasing the number of "
"elements or the size of the figure should fix this error."
msgstr ""

msgid ""
"Unable to create export file, please fill a bug report at "
"http://bugzilla.scilab.org."
msgstr ""

msgid "Scilab Graphic export module not installed.\n"
msgstr "O módulo de Exportação de gráfico do Scilab não está instalado.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr ""
"%s: Quantidade incorreta de argumentos de entrada: esperava-se de %d a %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um número "
"inteiro.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: Tipo incorreto para o argumento de entrada #%d: esperava-se um texto.\n"

#, c-format
msgid "%s: EMF format only available under Microsoft Windows OS.\n"
msgstr ""
"%s: O formato EMF está disponível apenas para o SO Microsoft Windows.\n"

#, c-format
msgid "%s: Unable to find pstoedit.\n"
msgstr "%s: Não conseguiu localizar pstoedit.\n"

#, c-format
msgid "%s: Unable to create export file, permission denied.\n"
msgstr "%s: Impossível criar o arquivo de exportação: permissão negada.\n"

#, c-format
msgid "%s: Unable to execute pstoedit.\n"
msgstr "%s: Não é possível executar pstoedit.\n"
