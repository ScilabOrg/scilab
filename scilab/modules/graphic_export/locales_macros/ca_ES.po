#, c-format
msgid ""
"%s: Wrong number of input argument: String expected with ''%s'', ''%s'', "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: El nombre d''arguments d''entrada és incorrecte: Ha de ser una cadena de "
"text amb els controladors ''%s'', ''%s'', ''%s'' o ''%s''.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr ""
"%s: Nombre incorrecte d''arguments d''entrada: Hi ha d''haver entre %d i "
"%d.\n"

#, c-format
msgid ""
"%s: Wrong number of input arguments: Function has no input argument with "
"''%s'' or ''%s'' drivers.\n"
msgstr ""
"%s: El nombre d''arguments d''entrada és incorrecte: La funció no té cap "
"argument d''entrada amb els controladors ''%s'' o ''%s''.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer expected.\n"
msgstr "%s: L''argument #%d és d''un tipus incorrecte: Ha de ser un enter.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr ""
"%s: L''argument d''entrada #%d és d''un tipus incorrecte: Ha de ser un "
"text.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: ''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: L''argument d''entrada #%d té un valor incorrecte: Ha de ser ''%s'' or "
"''%s''.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: ''%s'', ''%s'', ''%s'', ''%s'', "
"''%s'' or ''%s'' expected.\n"
msgstr ""
"%s: L''argument d''entrada #%d té un valor incorrecte: Ha de ser ''%s'', "
"''%s'', ''%s'', ''%s'', ''%s'' o ''%s''.\n"

