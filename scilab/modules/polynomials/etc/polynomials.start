function [polynomialslib, %s, %z, $] = startModule()

//Load  functions libraries
// =============================================================================
  load('SCI/modules/polynomials/macros/lib');

// Special variables definition
// =============================================================================
  %s = poly(0,'s');
  %z = poly(0,'z');
  $  = poly(0,'$');
  
endfunction

[polynomialslib, %s, %z, $] = startModule();
clear startModule;
