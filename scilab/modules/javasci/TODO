    @Test(sequential = true)
    public void typeMatrixTest() throws NullPointerException {
        sci = new Scilab();
        sci.open();
        sci.exec("a = 2*%pi");
        assert sci.getVariableType("a") == ScilabTypeEnum.sci_matrix;
        sci.close();
    }

    @Test(sequential = true)
    public void typeStringTest() throws NullPointerException {
        sci = new Scilab();
        sci.open();
        sci.exec("b = 'plop'");
        assert sci.getVariableType("b") == ScilabTypeEnum.sci_strings;
        sci.close();
    }

