// <-- Non-regression test for bug 745 -->
//
// <-- URL -->
//  http://forge.scilab.org/index.php/p/csv-readwrite/issues/745/
//
// <-- Short Description -->
// csv_textscan did no convert "1,1" into double.
// =============================================================================
// Example #1
M = [
"1,1000000000000001 0,10000000000000001"
"0,10000000000000001 1,1000000000000001"
"0,10000000000000001 0,10000000000000001"
];
A = csv_textscan(M, " ", ",");
// Example #2
csv_default("separator"," ");
csv_default("decimal",",");
x = csv_stringtodouble("1,1000000000000001");
assert_checkequal(x, 1.1000000000000001);
// Example #2
A = csv_textscan("1,1000000000000001", " ", ",");
assert_checkequal(A, 1.1000000000000001);
