#, c-format
msgid "%s: Slave interpreter already exists.\n"
msgstr "%s: допоміжний інтерпретатор вже запущено.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d to %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати від %d до "
"%d параметрів.\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d expected.\n"
msgstr "%s: помилкова кількість вихідних параметрів: мало бути %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A Boolean expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип "
"«boolean».\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A String expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип "
"«string».\n"

msgid "Color"
msgstr "Колір"

#, c-format
msgid "Error %s while loading library %s"
msgstr "Помилка %s під час спроби завантаження бібліотеки %s"

msgid "Puzzle"
msgstr "Складанка"

msgid "Scale"
msgstr "Масштаб"

msgid "Scroll"
msgstr "Гортати"
