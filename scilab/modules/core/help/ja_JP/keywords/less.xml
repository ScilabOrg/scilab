  <refnamediv>
    <refname>less</refname>
    <refpurpose> (&lt;) より小さいの比較</refpurpose>
  </refnamediv>
  <refnamediv xml:id="great">
    <refname>great</refname>
    <refpurpose> (&lt;) より大きいの比較</refpurpose>
  </refnamediv>
  <refsection>
    <title>説明</title>
    <para>
    論理値比較記号
  </para>
    <variablelist>
      <varlistentry>
        <term>&lt;&gt;</term>
        <listitem>
          <para>"異なる"を意味する (<literal>~=</literal>と同じ)</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>&lt;</term>
        <listitem>
          <para>"より小さい"を意味する</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>&gt;</term>
        <listitem>
          <para>means "より大きい"を意味する</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>&lt;=</term>
        <listitem>
          <para>より小さいか等しいを意味する.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>&gt;=</term>
        <listitem>
          <para>より大きいか等しいを意味する</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>
  <refsection role="see also">
