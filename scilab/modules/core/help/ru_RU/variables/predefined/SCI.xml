        <title>Смотрите также</title>
        <simplelist type="inline">
            <member>
                <link linkend="SCIHOME">SCIHOME</link>
            </member>
        </simplelist>
        <simplelist type="inline">
            <member>
                <link linkend="TMPDIR">TMPDIR</link>
            </member>
        </simplelist>
    </refsection>
</refentry>
