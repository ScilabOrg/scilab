// Copyright INRIA 2005
// Date : 6 mars 2006
a=1+-1;
if a<>0 then bugmes();quit;end
a=1-+1;
if a<>0 then bugmes();quit;end
a=1++1;
if a<>2 then bugmes();quit;end
a=1--1;
if a<>2 then bugmes();quit;end
a=1*-1;
if a<>-1 then bugmes();quit;end
a=1*+1;
if a<>1 then bugmes();quit;end
a=2*+++--2;
if a<>4 then bugmes();quit;end
a=3+-1*+++--2;
if a<>1 then bugmes();quit;end
