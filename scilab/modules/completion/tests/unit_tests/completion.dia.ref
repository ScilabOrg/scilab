// File paths
if (getos() == "Windows") then;
    r = completion('C:\Documents and');
    if r <> 'Documents and Settings\' then bugmes();quit;end
    r = completion('c:\documents and');
    if r <> 'Documents and Settings\' then bugmes();quit;end
else;
    r = completion('/tm');
    if r <> '/tmp' then bugmes();quit;end
    r = completion('/TM');
    if r <> '/tmp' then bugmes();quit;end
end;
r = completion(SCI+'/t');
if r <> ['thirdparty\'; 'tools\'] then bugmes();quit;end
// Predefined variables
