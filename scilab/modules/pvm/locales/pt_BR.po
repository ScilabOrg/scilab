#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Ocorreu um erro: %s\n"

#, c-format
msgid "%s: Error while creating processes: %s.\n"
msgstr "%s: Erro ao se criar processos: %s.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Memória esgotada.\n"

#, c-format
msgid "%s: No such file or directory.\n"
msgstr "%s: Não há tal arquivo ou diretório.\n"

#, c-format
msgid "%s: Unknown or not yet implemented type.\n"
msgstr "%s: Tipo desconhecido ou ainda não implementado.\n"

#, c-format
msgid "%s: Wrong first input argument: '%s' or '%s' expected.\n"
msgstr ""
"%s: Primeiro argumento de entrada errado: Esperava-se '%s' ou '%s'.\n"

#, c-format
msgid "%s: workspace (stacksize) is too small.\n"
msgstr ""
"%s: espaço de trabalho (tamanho da pilha - stacksize) muito pequeno.\n"

msgid "(group,instance) already exists"
msgstr "(grupo, instância) já existe"

msgid "Already doing operation"
msgstr "Já está fazendo a operação"

msgid "Already in group"
msgstr "Já está no grupo"

msgid "Bad message id"
msgstr "ID de mensagem ruim"

msgid "Bad parameter"
msgstr "Parâmetro ruim"

msgid "Barrier count mismatch"
msgstr "Não correspondência da contagem de barreira"

msgid "Can not decode received msg"
msgstr "Não se consegue a mensagem recebida"

msgid "Can not get memory"
msgstr "Não se consegue obter memória"

msgid "Daemon pvmd is not responding"
msgstr "Daemon pvmd não está respondendo"

msgid "Error MALLOC in pvm_error\n"
msgstr "Erro de MALLOC em pvm_error\n"

msgid "Error pvm_halt - signal\n"
msgstr "Erro em pvm_halt - sinal\n"

msgid "Error pvm_reduce: Not scalar type\n"
msgstr "Erro em pvm_reduce: Não é um escalar\n"

#, c-format
msgid "Error pvm_reduce: Unknown reduction operation %s\n"
msgstr "Erro em pvm_reduce: operação de redução desconhecida: %s\n"

msgid "Failed to exec new slave pvmd"
msgstr "Falhou ao executar um novo pvmd escravo"

msgid "Function not implemented"
msgstr "Função não implementada"

msgid "Host already configured (Duplicate host)"
msgstr "Host já configurado (host duplicado)"

msgid "Host failed"
msgstr "O host falhou"

msgid "No current buffer"
msgstr "Não há buffer corrente"

msgid "No group with that name"
msgstr "Não há grupo com tal nome"

msgid "No parent task"
msgstr "Sem tarefas pai"

msgid "No such (group,instance)"
msgstr "Não há tal (grupo, instância)"

msgid "No such executable"
msgstr "Não há tal executável"

msgid "No such host"
msgstr "Não há tal host"

msgid "No such instance in group"
msgstr "Não há tal exemplo no grupo"

msgid "No such task"
msgstr "Não há tal tarefa"

msgid "Not in group"
msgstr "Não está no grupo"

msgid "Null group name is illegal"
msgstr "Nome do grupo nulo é ilegal"

msgid "OK"
msgstr "OK"

msgid "Out of resources"
msgstr "Sem recursos"

msgid "PVM exists"
msgstr "O PVM existe"

msgid "PVM not found"
msgstr "PVM não encontrado"

msgid "PVM parallel Toolbox"
msgstr "Computações Paralelas com PVM"

msgid "Pvmd system error"
msgstr "Erro de sistema Pvmd"

msgid "Pvmd-pvmd protocol mismatch"
msgstr "O protocolo Pvmd-pvmd protocol não corresponde."

msgid "Read past end of buffer"
msgstr "Leu além do fim do buffer"

msgid "Scilab PVM module not installed.\n"
msgstr "O módulo Scilab PVM não está instalado.\n"

#, c-format
msgid "The configuration file %s is used.\n"
msgstr "O arquivo de configuração %s é usado.\n"

#, c-format
msgid ""
"The standard configuration file $SCI%s will be used.\n"
"With SCI=%s\n"
"SCI will have to be set on remote hosts\n"
"in order to spawn scilab\n"
msgstr ""
"O arquivo de configuração padrão $SCI%s será usado.\n"
"Com SCI=%s\n"
"SCI terá que ser ajustado em hosts remotos\n"
"Para se gerar o Scilab\n"

#, c-format
msgid ""
"The standard configuration file $SCI%s%s needs the environment variables %s, "
"%s, %s to be set. PVM can not be started.\n"
msgstr ""
"O arquivo de configuração padrão $SCI%s%s necessita das variáveis de "
"ambiente %s, %s, %s para ser ajustado. PVM não pode ser iniciado.\n"

msgid "Unknown Error"
msgstr "Erro desconhecido"

msgid "Warning: PVM_ROOT is not set.\n"
msgstr "Aviso: PVM_ROOT não foi ajustado.\n"

#, c-format
msgid "Warning: PVM_ROOT is set to %s\n"
msgstr "Aviso: PVM_ROOT está ajustado como %s\n"

#, c-format
msgid ""
"Warning: The standard configuration file $SCI%s was not found.\n"
"We supposed that PVM and scilab are in standard place on your net\n"
" (Cf. man pvmd3)\n"
msgstr ""
"Aviso: O arquivo de configuração padrão $SCI%s não foi encontrado.\n"
"Supõe-se que o PVM e o Scilab estejam em lugares padrões em sua rede\n"
" (Cf. man pvmd3)\n"

msgid "but there exists no configuration file:\n"
msgstr "mas não existe arquivo de configuração:\n"

