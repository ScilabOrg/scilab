#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Wystąpił błąd: %s\n"

#, c-format
msgid "%s: Error while creating processes: %s.\n"
msgstr "%s: Błąd podczas tworzenia procesów: %s.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Brak pamięci.\n"

#, c-format
msgid "%s: No such file or directory.\n"
msgstr "%s: Nie ma takiego pliku lub katalogu.\n"

#, c-format
msgid "%s: Unknown or not yet implemented type.\n"
msgstr "%s: Nieznany lub jeszcze nie zaimplementowany typ.\n"

#, c-format
msgid "%s: Wrong first input argument: '%s' or '%s' expected.\n"
msgstr ""
"%s: Nieprawidłowy pierwszy argument wejściowy: oczekiwany jest '%s' lub "
"'%s'.\n"

#, c-format
msgid "%s: workspace (stacksize) is too small.\n"
msgstr "%s: przestrzeń robocza (rozmiar stosu) zbyt mała.\n"

msgid "(group,instance) already exists"
msgstr "(Grupa, przykład) już istnieje"

msgid "Already doing operation"
msgstr "Operacja jest już wykonywana."

msgid "Already in group"
msgstr "Już w grupie"

msgid "Bad message id"
msgstr "Złe id wiadomości"

msgid "Bad parameter"
msgstr "Błędny parametr"

msgid "Barrier count mismatch"
msgstr "Niedopasowana liczba barier"

msgid "Can not decode received msg"
msgstr "Nie można rozkodować otrzymanej wiadomości"

msgid "Can not get memory"
msgstr "Nie można pobrać pamięci"

msgid "Daemon pvmd is not responding"
msgstr "Daemon pvmd nie odpowiada"

msgid "Error MALLOC in pvm_error\n"
msgstr "Błąd MALLOC w pvm_błąd\n"

msgid "Error pvm_halt - signal\n"
msgstr "Błąd pvm_zablokowanie - sygnału\n"

msgid "Error pvm_reduce: Not scalar type\n"
msgstr "Błąd pvm_redfuce: nie jest typu skalarnego.\n"

#, c-format
msgid "Error pvm_reduce: Unknown reduction operation %s\n"
msgstr "Błąd pvm_reduce: nieznana operacja redukcji %s\n"

msgid "Failed to exec new slave pvmd"
msgstr "Nie można wykonać nowego slave PVMD"

msgid "Function not implemented"
msgstr "Funkcja nie zostanie wykonana"

msgid "Host already configured (Duplicate host)"
msgstr "Host już skonfigurowany (duplikat hosta)"

msgid "Host failed"
msgstr "błąd użytkownika"

msgid "No current buffer"
msgstr "Brak bieżącego bufora"

msgid "No group with that name"
msgstr "Brak grupy z tą nazwą"

msgid "No parent task"
msgstr "Brak zadania rodziców"

msgid "No such (group,instance)"
msgstr "Nie ma takiego (grupy, przypadku)"

msgid "No such executable"
msgstr "Nie ma takiego wykonywalnego pliku"

msgid "No such host"
msgstr "Nie ma takiego hosta"

msgid "No such instance in group"
msgstr "Nie ma takiego przypadku w grupie"

msgid "No such task"
msgstr "Nie ma takiego zadania"

msgid "Not in group"
msgstr "Nie w grupie"

msgid "Null group name is illegal"
msgstr "Nazwa grupy: null jest nie dozwolona"

msgid "OK"
msgstr "Ok"

msgid "Out of resources"
msgstr "Brak zasobów"

msgid "PVM exists"
msgstr "PVM istnieje"

msgid "PVM not found"
msgstr "Nie odnaleziono PVM"

msgid "PVM parallel Toolbox"
msgstr "PVM równoległe Toolbox"

msgid "Pvmd system error"
msgstr "Pvmd błąd systemu"

msgid "Pvmd-pvmd protocol mismatch"
msgstr "Pvmd-pvmd niedopasowany protokół"

msgid "Read past end of buffer"
msgstr "Czytanie po zakończeniu bufora"

msgid "Scilab PVM module not installed.\n"
msgstr "Moduł PVM nie zainstalowany.\n"

#, c-format
msgid "The configuration file %s is used.\n"
msgstr "Plik konfiguracyjny %s jest używany.\n"

#, c-format
msgid ""
"The standard configuration file $SCI%s will be used.\n"
"With SCI=%s\n"
"SCI will have to be set on remote hosts\n"
"in order to spawn scilab\n"
msgstr ""
"Standardowa konfiguracja pliku $SCI%s zostanie użyta.\n"
"Z SCI=%s\n"
"SCI będzie musiała być ustawiona na zdalnym użytkowniku\n"
"w poleceniu spawn scilab\n"

#, c-format
msgid ""
"The standard configuration file $SCI%s%s needs the environment variables %s, "
"%s, %s to be set. PVM can not be started.\n"
msgstr ""
"Standardowa konfiguracja pliku $SCI%s%s potrzebne środowisko "
"zmiennych%s,%s,%s do rozwiązania. PVM nie można uruchomić.\n"

msgid "Unknown Error"
msgstr "Nieznany błąd"

msgid "Warning: PVM_ROOT is not set.\n"
msgstr "Uwaga: PVM_ROOT nie jest ustawiony.\n"

#, c-format
msgid "Warning: PVM_ROOT is set to %s\n"
msgstr "Uwaga: PVM_ROOT jest ustawiony do %s\n"

#, c-format
msgid ""
"Warning: The standard configuration file $SCI%s was not found.\n"
"We supposed that PVM and scilab are in standard place on your net\n"
" (Cf. man pvmd3)\n"
msgstr ""
"Ostrzeżenie: Standardowy plik konfiguracyjny $SCI%s nie został odnaleziony.\n"
"Możemy przypuszczać że PVM i scilab są w standardowym miejscu w twojej "
"sieci.\n"
" (Cf. man pvmd3)\n"

msgid "but there exists no configuration file:\n"
msgstr "ale nie istnieje plik konfiguracyjny:\n"

