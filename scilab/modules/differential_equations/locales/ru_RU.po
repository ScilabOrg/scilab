#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s: Неверное количество входных аргументов: ожидалось %d.\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d expected.\n"
msgstr "%s: Неверное количество выходных аргументов: ожидалось %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A matrix expected.\n"
msgstr "%s: Неверный тип входного аргумента №%d: ожидалась матрица.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A scalar expected.\n"
msgstr "%s: Неверный тип входного аргумента №%d: ожидался скаляр.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Value at most 20 expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: ожидалось не более 20.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A vector of size %d (N) expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидался вектор размером %d "
"(N).\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Sum of m (%d) must be less than 40.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: сумма m (%d) должна быть "
"меньше, чем 40.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: zeta(j) lower or equal to zeta(j+1) "
"expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: ожидалось zeta(j) меньшим либо "
"равным zeta(j+1).\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A vector of size 11 expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: ожидался вектор размером 11.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Wrong value for element %s: %d or %d "
"expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: неверное значение элемента %s: "
"ожидалось %d или %d.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Wrong value for element %s: %d to %d "
"expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: неверное значение элемента %s: "
"ожидалось от %d до %d.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Wrong value for element %s: %d to "
"sum of m (=%d) expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: неверное значение элемента %s: "
"ожидалось от %d до суммы m (=%d).\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Wrong value for element %d.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: неверное значение элемента "
"%d.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Wrong value for element %d: %d to %d "
"expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: неверное значение элемента %d: "
"ожидалось от %d до %d.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: An array of size %d (ipar(4)) "
"expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидался массив размером %d "
"(ipar(4)).\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: %d to sum of m (=%d) expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: ожидалось от %d до суммы m "
"(=%d).\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Bad value for ltol(%d): ltol(1) < "
"ltol(2) < ... < ltol(NTOL) <= M (sum of m) expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: плохое значение для ltol(%d): "
"ожидалось ltol(1) < ltol(2) < ... < ltol(NTOL) <= M (сумма по m).\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: An array of size %d (ipar(11)) "
"expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидался массив размером %d "
"(ipar(11)).\n"

#, c-format
msgid "%s: Subroutine not found: %s\n"
msgstr "%s: Подпрограмма не найдена: %s\n"

#, c-format
msgid "%s: Argument #%d: Subroutine not found in list: %s\n"
msgstr "%s: Аргумент №%d: подпрограмма не найдена в списке: %s\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: The first argument in the list must "
"be a Scilab function.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: первый аргумент в списке должен "
"быть функцией Scilab.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A function expected.\n"
msgstr "%s: Неверный тип входного параметра №%d: ожидалась функция.\n"

#, c-format
msgid "%s: An error occured in '%s' subroutine.\n"
msgstr "%s: В подпрограмме '%s' произошла ошибка.\n"

#, c-format
msgid "%s: The collocation matrix is singular.\n"
msgstr "%s: Матрица коллокации сингулярна.\n"

#, c-format
msgid ""
"%s: The expected no. of subintervals exceeds storage specifications.\n"
msgstr ""
"%s: Ожидаемое количество подынтервалов превысило спецификации запоминающего "
"устройства.\n"

#, c-format
msgid "%s: The nonlinear iteration has not converged.\n"
msgstr "%s: Нелинейная итерация не сошлась.\n"

#, c-format
msgid "%s: There is an input data error.\n"
msgstr "%s: Ошибка во входных данных.\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d to %d expected.\n"
msgstr ""
"%s: Неверное количество выходных аргументов: ожидалось от %d до %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A real matrix expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: ожидалась вещественная матрица.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A real matrix with %d to %d column(s) "
"expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидалась вещественная матрица, "
"у которой от %d до %d столбцов.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
msgstr "%s: Неверный размер входного аргумента №%d: ожидался скаляр.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A scalar or a matrix of size %d "
"expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидался скаляр или матрица "
"размером %d.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Atol and Rtol must have the same "
"size.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: Atol и Rtol должны быть "
"одинакового размера.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A matrix or a list expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: ожидалась матрица или список.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Argument %d in the list must be a "
"matrix.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: аргумент %d в списке должен быть "
"матрицей.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A list of size %d expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидался список размером %d.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Element %d in the info list must be a "
"scalar.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: элемент %d в списке info должен "
"быть скаляром.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Element %d in the info list must be a "
"matrix.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: элемент %d в списке info должен "
"быть матрицей.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Argument %d in the list must be of "
"size %d.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: аргумент %d в списке должен быть "
"размером %d.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: The first argument in the list must "
"be a string, a function or a matrix in case of argument info.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: первый аргумент в списке должен "
"быть строкой, функцией или матрицей в случае аргумента info.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A matrix or a function expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: ожидалась матрица или функция.\n"

#, c-format
msgid "%s: Wrong size for input argument(s) %d: %d expected.\n"
msgstr "%s: Неверный размер входного аргумента %d: ожидалось %d.\n"

#, c-format
msgid "%s: %s return with state %d.\n"
msgstr "%s: %s возвращено с состоянием %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d : A real matrix expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d : ожидалась вещественная матрица.\n"

#, c-format
msgid "%s: Argument #%d : Subroutine not found in list: %s\n"
msgstr "%s: Аргумент %d : в списке не найдена подпрограмма: %s\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d : The first argument in the list must "
"be a Scilab function.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d : первый аргумент в списке должен "
"быть функцией Scilab.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d : A function expected.\n"
msgstr "%s: Неверный тип входного аргумента №%d : ожидалась функция.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: It must be one of the following "
"strings: adams or stiff.\n"
msgstr ""
"%s: Неверное значение аргумента №%d: оно должно быть одной из следующих "
"строк: adams или stiff.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A real vector expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: ожидался вещественный вектор.\n"

#, c-format
msgid "%s: Arg %d and arg %d must have equal dimensions.\n"
msgstr "%s: Аргумент %d и аргумент %d должны иметь одинаковые размеры.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: The first argument in the list must "
"be a string or a function.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: первый аргумент в списке должен "
"быть строкой или функцией.\n"

#, c-format
msgid "%s: Wrong size for w and iw: w = %d and iw = %d expected.\n"
msgstr "%s: Неверный размер w и iw: ожидалось w = %d и iw = %d.\n"

msgid ""
"The user-supplied subroutine res signalled lsodi to halt the integration and "
"return (ires=2). Execution of the external function has failed.\n"
msgstr ""
"Пользовательская подпрограмма res выдала сигнал lsodi для остановки "
"интеграции и возврата (ires=2). Исполнение внешней функции завершилось с "
"ошибкой.\n"

#, c-format
msgid "%s: %s exit with state %d.\n"
msgstr "%s: %s вышло с состоянием %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A 3 by N matrix expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидалась матрица 3 на N.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Same size of input argument %d "
"expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидался такой же размер, как у "
"аргумента %d.\n"

#, c-format
msgid ""
"%s: Argument #%d: Variable returned by scilab argument function is "
"incorrect.\n"
msgstr ""
"%s: Аргумент №#%d: переменная, возвращёная функцией аргумента scilab "
"некорректна.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: %d expected.\n"
msgstr "%s: Неверный размер входного аргумента №%d: ожидался размер %d.\n"

#, c-format
msgid ""
"%ls: Warning: Wrong value for the first element of argument #%d: The default "
"value will be used.\n"
msgstr ""
"%ls: Предупреждение: неверное значение первого элемента аргумента №%d: будет "
"использоваться значение по умолчанию.\n"

#, c-format
msgid ""
"%ls: Warning: Wrong value for the third element of argument #%d: The default "
"value will be used.\n"
msgstr ""
"%ls: Предупреждение: неверное значение третьего элемента аргумента №%d: "
"будет использоваться значение по умолчанию.\n"

#, c-format
msgid ""
"%ls: Warning: Wrong value for the fourth element of argument #%d: The "
"default value will be used.\n"
msgstr ""
"%ls: Предупреждение: неверное значение четвёртого элемента аргумента №%d: "
"будет использоваться значение по умолчанию.\n"

#, c-format
msgid "%s: Means termination for lack of space to divide another triangle.\n"
msgstr ""
"%s: Завершение усреднения из-за нехватки пространства для деления другого "
"треугольника.\n"

#, c-format
msgid "%s: Means termination because of roundoff noise.\n"
msgstr "%s: Завершение усреднения из-за шума округления.\n"

#, c-format
msgid "%s: means termination with relative error <= 5.0* machine epsilon.\n"
msgstr ""
"%s: завершение усреднения с относительной ошибкой <= 5.0* машинная эпсилон.\n"

#, c-format
msgid ""
"%s: Means termination because the number of function evaluations has "
"exceeded MEVALS.\n"
msgstr ""
"%s: Завершение усреднения из-за того, что количество исполнений функций "
"превысило MEVALS.\n"

#, c-format
msgid "%s: Means termination because of error in input flag.\n"
msgstr "%s: Завершение усреднений из-за ошибки во входном флаге.\n"

#, c-format
msgid "%s: twodq return with state %d.\n"
msgstr "%s: twodq return with state %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A 4 by N matrix expected.\n"
msgstr ""
"%s: Неверный размер входного аргумента №%d: ожидалась матрица 4 на N.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: A positive value expected.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: ожидалось положительное "
"значение.\n"

#, c-format
msgid ""
"%ls: Warning: Wrong value for the element %d of argument #%d: The default "
"value will be used.\n"
msgstr ""
"%ls: Предупреждение: неверное значение элемента %d аргумента №%d: будет "
"использоваться значение по умолчание.\n"

#, c-format
msgid ""
"%ls: Warning: Wrong value for the element %d and %d of argument #%d: The "
"default value will be used.\n"
msgstr ""
"%ls: Предупреждение: неверное значение элемента  %d и %d аргумента №%d: "
"будет использоваться значение по умолчанию.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: minpts smaller than maxpts "
"expected.\n"
msgstr ""
"%s: Неверное значение аргумента №%d: ожидалось, что minpts меньше, чем "
"maxpts.\n"

#, c-format
msgid "%ls: Warning: maxpts was too small to obtain the required accuracy.\n"
msgstr ""
"%ls: Предупреждение: maxpts было слишком мало, чтобы получить требуемую "
"точность.\n"

#, c-format
msgid "%s: The volume of one of the initially given tetrahedrons is zero.\n"
msgstr "%s: Объём одного из исходно заданных четырёхгранников равен нулю.\n"

#, c-format
msgid "%s: dcutet return with error %d.\n"
msgstr ""

#, c-format
msgid "%s: Wrong type for input argument #%d: Must not contain NaN or Inf.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: не должен содержать NaN или Inf.\n"

#, c-format
msgid ""
"%s: Maximum number of subdivisions achieved. Splitting the interval might "
"help.\n"
msgstr ""
"%s: Достигнуто максимальное количество дроблений. Может помочь разделение "
"интервала.\n"

#, c-format
msgid ""
"%s: Round-off error detected, the requested tolerance (or default) cannot be "
"achieved. Try using bigger tolerances.\n"
msgstr ""
"%s: Обнаружена ошибка округления, требуемый допуск (или значение по "
"умолчанию) не может быть достигнут. Попробуйте увеличить допуски.\n"

#, c-format
msgid ""
"%s: Bad integrand behavior occurs at some points of the integration "
"interval.\n"
msgstr ""
"%s: Плохое поведение integrand  в некоторых точках интервала "
"интегрирования.\n"

#, c-format
msgid ""
"%s: Convergence problem, round-off error detected. Try using bigger "
"tolerances.\n"
msgstr ""
"%s: Проблема сходимость, обнаружена ошибка округления. Попробуйте увеличить "
"допуски.\n"

#, c-format
msgid "%s: The integral is probably divergent, or slowly convergent.\n"
msgstr "%s: Интеграл вероятно расходится или медленно сходится.\n"

#, c-format
msgid ""
"%s: Invalid input, absolute tolerance <= 0 and relative tolerance < 2.e-14.\n"
msgstr ""
"%s: Недопустимый вход, абсолютный допуск  <= 0, а относительный допуск < 2.e-"
"14.\n"

#, c-format
msgid "%s: Convergence problem...\n"
msgstr "%s: Проблема сходимости...\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: It must be one of the following "
"strings: adams, stiff, rk, rkf, fix, root or discrete.\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: должно быть одно из следующих "
"строковых значений: adams, stiff, rk, rkf, fix, root или discrete.\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d, %d or %d expected.\n"
msgstr ""
"%s: Неверное количество выходных аргументов: ожидалось %d, %d или %d.\n"

#, c-format
msgid ""
"%s: Wrong value of hmin and hmax: hmin = %d is greater than hmax = %d.\n"
msgstr ""
"%s: Неверное значение hmin и hmax: hmin = %d больше, чем hmax = %d.\n"

#, c-format
msgid ""
"%s: Wrong value of Jacobian type: A number between %d and %d expected.\n"
msgstr ""
"%s: Неверное значение типа якобиана: ожидалось число между %d и %d.\n"

#, c-format
msgid "%s: Warning: Jacobian is given, but not used.\n"
msgstr "%s: Предупреждение: якобиан указан, но не используется.\n"

msgid ""
"%s: Warning: No Jacobian external given, but one is required by "
"%ODEOPTIONS(6) value. Jacobian will be estimated.\n"
msgstr ""
"%s: Предупреждение: Не указан внешний якобиан, но якобиан требуется "
"значением %ODEOPTIONS(6). Якобиан будет оценён.\n"

#, c-format
msgid ""
"%s: Warning: Wrong value for maximum stiff/non-stiff order allowed :\n"
"At most %d for mxordn, %d for mxords and no null value for both expected.\n"
"Wrong value will be reduced to the default value.\n"
msgstr ""
"%s: Предупреждение: неверное значение разрешённого максимального "
"жёсткого/нежёсткого порядка :\n"
"ожидалось не более %d для mxordn, %d для mxords и ненулевое для обоих.\n"
"Неверное значение будет уменьшено до значения по умолчанию.\n"

#, c-format
