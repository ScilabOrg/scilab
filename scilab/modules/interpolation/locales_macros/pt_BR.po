#, c-format
msgid "%s: Wrong number of input arguments: Must be between %d and %d.\n"
msgstr ""
"%s: Quantidade errada de argumentos de entrada: Deve estar enre %d e %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Two rows expected.\n"
msgstr ""
"%s: Tamanho errado para o argumento de entrada #%d: Esperava-se duas "
"linhas.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Vector expected.\n"
msgstr ""
"%s: Tamanho errado para o argumento de entrada #%d: Esperava-se um vetor.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Vector or matrix expected.\n"
msgstr ""
"%s: Tamanho errado para o argumento de entrada #%d: Esperava-se um vetor ou "
"uma matriz.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d: Same size expected.\n"
msgstr ""
"%s: Tamanho errado para os argumentos de entrada #%d e #%d: Esperava-se "
"tamanhos iguais.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Array of reals expected.\n"
msgstr ""
"%s: Tipo errado para o argumento de entrada #%d: Esperava-se array de "
"reais.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: ''%s'' or ''%s'' or ''%s'' "
"expected.\n"
msgstr ""
"%s: Valor errado para o argumento de entrada #%d: Esperava-se ''%s'' ou "
"''%s'' ou ''%s''.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: ''%s'' or real expected.\n"
msgstr ""
"%s: Valor errado para o argumento de entrada #%d: Esperava-se ''%s'' ou "
"real.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Reals expected but some NaN found.\n"
msgstr ""
"%s: Valor errado para o argumento de entrada #%d: Esperava-se reais, mas "
"foram encontrados alguns NaN.\n"
