#, c-format
msgid "%s: Wrong size for input arguments.\n"
msgstr "%s：输入参数的大小错误。\n"

#, c-format
msgid ""
"%s: x and/or y are not in strict increasing order (or +-inf detected)\n"
msgstr "%s：x和（或）y不是严格递增排序（或者存在+-inf）\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Unsupported 'spline' type.\n"
msgstr "%s：输入参数#%d的值错误：不支持'spline'类型。\n"

#, c-format
msgid ""
"%s: Incompatible input arguments '#%d', '#%d' and '#%d': Same sizes "
"expected.\n"
msgstr "%s：不兼容的输入参数'#%d'，'#%d'和'#%d'：应该为同样大小。\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: tensbs3d tlist expected.\n"
msgstr "%s：输入参数#%d的类型错误：应该为tensbs3d tlist。\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Unsupported 'outmode' type.\n"
msgstr "%s：输入参数#%d的值错误：不支持'outmode'类型。\n"

#, c-format
msgid "%s: xyz must be a (n,3) real matrix with n >= 10\n"
msgstr "%s：xyz必须是一个大小为(n,3)的实数矩阵，且n>=10\n"

#, c-format
msgid "%s: duplicate nodes or all nodes colinears (ier = %d).\n"
msgstr "%s：重复的节点（node）或所有节点同线性（colinears）（ier=%d）。\n"

#, c-format
msgid ""
"%s: Incompatible input arguments '#%d' and '#%d': Same sizes expected.\n"
msgstr "%s：不兼容的输入参数'#%d'和'#%d'：应该为同样大小。\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Unknown 'outmode' type.\n"
msgstr "%s：输入参数#%d的值错误：未知的'outmode'类型。\n"

#, c-format
msgid "%s: the x, y and z grids must have at least 3 points\n"
msgstr "%s：x、y和z格（grids）必须至少有3个点。\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A real 3-dimension hypermatrix "
"expected.\n"
msgstr "%s：输入参数#%d的大小错误：应该为一个三维实数多维矩阵。\n"

#, c-format
msgid "%s: Input argument #%d must be a vector with 3 components.\n"
msgstr "%s：输入参数#%d必须是一个具有3个元素的向量。\n"

#, c-format
msgid "%s: Wrong values for input argument #%d.\n"
msgstr "%s：输入参数#%d的值错误。\n"

#, c-format
msgid "%s: Problem with 'flag' = %d\n"
msgstr "%s：错误发生在'flag'=%d\n"

#, c-format
msgid ""
"%s: Incompatible input arguments '#%d and '#%d': Same sizes expected.\n"
msgstr "%s：输入参数#%d和#%d不兼容：应该具有同样大小。\n"

#, c-format
msgid "%s: bad input for arg 3\n"
msgstr "%s：第三个参数输入错误。\n"

#, c-format
msgid "%s: bad input for x\n"
msgstr "%s：x的输入错误。\n"

#, c-format
msgid ""
"%s: elements of argument %d not (strictly) increasing or +-inf detected.\n"
msgstr "%s：参数%d的元素不是（严格）递增排序或者存在+-inf。\n"

#, c-format
msgid "%s: not enought points for the fit.\n"
msgstr "%s：拟合的点数不够。\n"

#, c-format
msgid "%s warning: rank deficiency of the least square matrix.\n"
msgstr "%s 警告：最小方阵的秩不足。\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: cshep2d tlist expected.\n"
msgstr "%s：输入参数#%d的类型错误：应该为cshep2d tlist。\n"

#, c-format
msgid "%s: Wrong size for input arguments: Vector of same size expected.\n"
msgstr "%s：输入参数的大小错误：应该为同样大小的向量。\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Must be >= %d.\n"
msgstr "%s：输入参数#%d的大小错误：必须 >= %d。\n"

#, c-format
msgid ""
"%s: elements of argument #%d not (strictly) increasing or +-inf detected\n"
msgstr "%s：参数#%d的元素不是（严格）递增排序或者存在+-inf。\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Unknown 'spline' type.\n"
msgstr "%s：输入参数#%d的值错误：未知的'spline'类型。\n"

#, c-format
msgid "%s: for a clamped spline you must give the endpoint slopes.\n"
msgstr "%s：对于一个clamped样条你必须指定endpoint slopes。\n"

#, fuzzy, c-format
msgid "%s: Wrong size for input argument #%d: endpoint slopes.\n"
msgstr "%s：输入参数#%d的大小错误：endpoint slopes。\n"

#, c-format
msgid "%s: Wrong number of input argument(s).\n"
msgstr "%s：输入参数的数目错误。\n"

#, c-format
msgid "%s: Wrong value for periodic spline y(1): Must be equal to y(n).\n"
msgstr "%s：periodic样条y(1)的值错误：必须等于y(n)。\n"

#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s：输入参数#%d的大小错误。\n"

#, c-format
msgid "%s: Grid abscissae of dim %d not in strict increasing order.\n"
msgstr "%s：第%d维的网格横坐标不是严格递增排序。\n"

#, c-format
msgid "%s: U must be a real %d-dim hypermatrix.\n"
msgstr "%s：U必须是一个%d维实数多维矩阵。\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and grid values in dimension "
"%d.\n"
msgstr "%s：在维度%d的网格点和网格值的大小不兼容。\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and values in dimension %d.\n"
msgstr "%s：在维度%d的网格点和值的大小不兼容。\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and values in dim %d or %d.\n"
msgstr "%s：在维度%d或维度%d的网格点和值的大小不兼容。\n"

#, c-format
msgid "Argument %d is not a real hypermatrix.\n"
msgstr "参数%d不是一个实数多维矩阵。\n"

#, c-format
msgid "Argument %d is not a string.\n"
msgstr "参数%d不是一个字符串。\n"
