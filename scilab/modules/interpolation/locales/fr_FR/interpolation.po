#, c-format
msgid "%s: Wrong size for input arguments.\n"
msgstr "%s: Mauvaise dimension pour les arguments d'entrée.\n"

#, c-format
msgid ""
"%s: x and/or y are not in strict increasing order (or +-inf detected)\n"
msgstr ""
"%s: x et/ou y ne sont pas rangés dans un ordre strictement croissant (ou +-"
"inf détecté)\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Unsupported 'spline' type.\n"
msgstr ""
"%s: Mauvaise valeurs pour l'argument d'entrée n° %d: Type 'spline' non "
"supporté.\n"

#, c-format
msgid ""
"%s: Incompatible input arguments '#%d', '#%d' and '#%d': Same sizes "
"expected.\n"
msgstr ""
"%s: Arguments d'entrée incompatibles '#%d', '#%d' et '#%d': Même dimension "
"attendue.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: tensbs3d tlist expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n° %d: tlist tensbs3d attendue.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Unsupported 'outmode' type.\n"
msgstr ""
"%s: Mauvaise valeurs pour l'argument d'entrée n° %d: Type 'outmode' non "
"supporté.\n"

#, c-format
msgid "%s: xyz must be a (n,3) real matrix with n >= 10\n"
msgstr "%s: xyz doit être une (n,3) matrice réelle avec n >= 10\n"

#, c-format
msgid "%s: duplicate nodes or all nodes colinears (ier = %d).\n"
msgstr "%s: noeuds dupliqués ou tous les noeuds colinéaires (ier = %d).\n"

#, c-format
msgid ""
"%s: Incompatible input arguments '#%d' and '#%d': Same sizes expected.\n"
msgstr ""
"%s: Arguments d'entrée incompatibles '#%d' et '#%d': Même dimension "
"attendue.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Unknown 'outmode' type.\n"
msgstr ""
"%s: Mauvaise valeurs pour l'argument d'entrée n° %d: Type 'outmode' "
"inconnu.\n"

#, c-format
msgid "%s: the x, y and z grids must have at least 3 points\n"
msgstr "%s: les grilles x, y et z doivent avoir au moins 3 points\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A real 3-dimension hypermatrix "
"expected.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée n° %d: Une hypermatrice "
"réelle à 3  dimensions attendue.\n"

#, c-format
msgid "%s: Input argument #%d must be a vector with 3 components.\n"
msgstr ""
"%s: Argument d'entrée n° %d doit être un vecteur avec 3 composantes.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d.\n"
msgstr "%s: Mauvaises valeurs pour l'argument d'entrée n° %d.\n"

#, c-format
msgid "%s: Problem with 'flag' = %d\n"
msgstr "%s: Problème avec 'flag' = %d\n"

#, c-format
msgid ""
"%s: Incompatible input arguments '#%d and '#%d': Same sizes expected.\n"
msgstr ""
"%s: Arguments d'entrée n° %d et n° %d incompatiblesd: Même tailles "
"attendues.\n"

#, c-format
msgid "%s: bad input for arg 3\n"
msgstr "%s: mauvaise entrée pour l'argument 3\n"

#, c-format
msgid "%s: bad input for x\n"
msgstr "%s: mauvaise entrée pour x\n"

#, c-format
msgid ""
"%s: elements of argument %d not (strictly) increasing or +-inf detected.\n"
msgstr ""
"%s: éléments de l'argument %d non (strictement) croissants ou +-inf "
"détectés.\n"

#, fuzzy, c-format
msgid "%s: not enought points for the fit.\n"
msgstr "%s: pas assez de points pour l'ajustement.\n"

#, c-format
msgid "%s warning: rank deficiency of the least square matrix.\n"
msgstr "%s attention: défaut de rang pour la matrice des moindres carrés.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: cshep2d tlist expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n° %d: tlist cshep2d attendue.\n"

#, c-format
msgid "%s: Wrong size for input arguments: Vector of same size expected.\n"
msgstr ""
"%s: Mauvaise dimension pour les arguments d'entrée: Vecteur de même "
"dimension attendu.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Must be >= %d.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée n° %d: Doit être >= %d.\n"

#, c-format
msgid ""
"%s: elements of argument #%d not (strictly) increasing or +-inf detected\n"
msgstr ""
"%s: éléments de l'argument n° %d ne sont pas dans une ordre (strictement) "
"croissant ou +-inf détecté\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Unknown 'spline' type.\n"
msgstr ""
"%s: Mauvaise valeurs pour l'argument d'entrée n° %d: Type 'spline' inconnu.\n"

#, fuzzy, c-format
msgid "%s: for a clamped spline you must give the endpoint slopes.\n"
msgstr ""
"%s: pour une spline encastrée, vous devez donner les pentes du point final.\n"

#, fuzzy, c-format
msgid "%s: Wrong size for input argument #%d: endpoint slopes.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée n° %d: pentes des points "
"terminaux.\n"

#, c-format
msgid "%s: Wrong number of input argument(s).\n"
msgstr "%s : Mauvais nombre d'argument(s) en entrée.\n"

#, c-format
msgid "%s: Wrong value for periodic spline y(1): Must be equal to y(n).\n"
msgstr ""
"%s: Mauvaise valeur pour la spline périodique y(1): Doit être égal à y(n).\n"

#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s: Mauvaise dimension pour l'argument d'entrée n° %d.\n"

#, fuzzy, c-format
msgid "%s: Grid abscissae of dim %d not in strict increasing order.\n"
msgstr ""
"%s: Les abscisses de la dimension %d ne sont pas en ordre strictement "
"croissant.\n"

#, c-format
msgid "%s: U must be a real %d-dim hypermatrix.\n"
msgstr "%s: U doit être une hypermatrice réelle à %d dimensions.\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and grid values in dimension "
"%d.\n"
msgstr ""
"%s: Incompatibilité de dimension entre les points de la grille et les "
"valeurs de la grille sur la dimension %d.\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and values in dimension %d.\n"
msgstr ""
"%s: Incompatibilité de dimension entre les points et les valeurs de la "
"grille sur la dimension %d.\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and values in dim %d or %d.\n"
msgstr ""
"%s: Incompatibilité de dimension entre les points et les valeurs de la "
"grille sur la dimension %d our %d.\n"

#, c-format
msgid "Argument %d is not a real hypermatrix.\n"
msgstr "Argument %d n'est pas une hypermatrice réelle.\n"

#, c-format
msgid "Argument %d is not a string.\n"
msgstr "Argument %d n'est pas une chaîne de caractères.\n"
