msgid ""
"\n"
"Largest element in matrix = %-1.4lg.\n"
msgstr ""
"\n"
"Найбільший елемент у матриці = %-1.4lg.\n"

#, c-format
msgid "Smallest element in matrix = %-1.4lg.\n"
msgstr "Найменший елемент матриці = %-1.4lg.\n"

#, c-format
msgid ""
"\n"
"Largest diagonal element = %-1.4lg.\n"
msgstr ""
"\n"
"Найбільший діагональний елемент = %-1.4lg.\n"

#, c-format
msgid "Smallest diagonal element = %-1.4lg.\n"
msgstr "Найменший діагональний елемент = %-1.4lg.\n"

#, c-format
msgid ""
"\n"
"Largest pivot element = %-1.4lg.\n"
msgstr ""
"\n"
"Найбільший опорний елемент = %-1.4lg.\n"

#, c-format
msgid "Smallest pivot element = %-1.4lg.\n"
msgstr "Найменший опорний елемент = %-1.4lg.\n"

#, c-format
msgid ""
"\n"
"Density = %2.2lf%%.\n"
msgstr ""
"\n"
"Щільність = %2.2lf%%.\n"

#, c-format
msgid "Number of fill-ins = %1d.\n"
msgstr "Кількість ненульових елементів = %1d.\n"

#, c-format
msgid "Warning : The following matrix is factored in to LU form.\n"
msgstr "Попередження: наведену нижче матрицю факторизовано за формулою LU.\n"

#, c-format
msgid "Matrix has not been factored.\n"
msgstr "Матрицю не було факторизовано.\n"

#, c-format
msgid "|||  Starting new matrix  |||\n"
msgstr "|||  Початок нової матриці  |||\n"

#, c-format
msgid "Matrix is complex.\n"
msgstr "Матриця є комплексною.\n"

#, c-format
msgid "Matrix is real.\n"
msgstr "Матриця є дійсною.\n"

#, c-format
msgid "     Initial number of elements = %d\n"
msgstr "     Початкова кількість елементів = %d\n"

#, c-format
msgid "     Initial average number of elements per row = %lf\n"
msgstr "     Початкова кількість елементів на рядок = %lf\n"

#, c-format
msgid "     Fill-ins = %d\n"
msgstr "     Ненульових коефіцієнтів = %d\n"

#, c-format
msgid "     Average number of fill-ins per row = %lf%%\n"
msgstr "     Середня кількість ненульових коефіцієнтів на рядок = %lf%%\n"

#, c-format
msgid "     Total number of elements = %d\n"
msgstr "     Загальна кількість елементів = %d\n"

#, c-format
msgid "     Average number of elements per row = %lf\n"
msgstr "     Середня кількість елементів на рядок = %lf\n"

#, c-format
msgid "     Density = %lf%%\n"
msgstr "     Щільність = %lf%%\n"

#, c-format
msgid "     Relative Threshold = %e\n"
msgstr "     Відносне порогове значення = %e\n"

#, c-format
msgid "     Absolute Threshold = %e\n"
msgstr "     Абсолютне порогове значення = %e\n"

#, c-format
msgid "     Largest Element = %e\n"
msgstr "     Найбільший елемент = %e\n"

#, c-format
msgid ""
"     Smallest Element = %e\n"
"\n"
"\n"
msgstr ""
"     Найменший елемент = %e\n"
"\n"
"\n"

#, c-format
msgid "Norm of right-hand side : %s\n"
msgstr "Норма правої частини: %s\n"

#, c-format
msgid "Special processing where the right-hand side is zero.\n"
msgstr "Особлива обробка для матриць з нульовою правою частиною.\n"

#, c-format
msgid "  Type of preconditionning #1 : %d\n"
msgstr "  Тип передобумовленості №1: %d\n"

#, c-format
msgid "  Type of preconditionning #2 : %d\n"
msgstr "  Тип передобумовленості №2: %d\n"

#, c-format
msgid "  New residual = %s < tol = %s => break\n"
msgstr "  Нова нев’язка = %s < tol = %s => зупинка\n"

#, c-format
msgid "  Iteration #%s/%s residual : %s\n"
msgstr "  Ітерація №%s з %s, нев’язка: %s\n"

#, c-format
msgid "Final residual = %s > tol =%s\n"
msgstr "Остаточна нев’язка = %s > припустиму похибку =%s\n"

#, c-format
msgid "Algorithm fails\n"
msgstr "Збій алгоритму\n"

#, c-format
msgid "%s: Convergence error.\n"
msgstr "%s: помилка збіжності.\n"

#, c-format
msgid "Algorithm pass\n"
msgstr "Алгоритм пройдено\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати від %d до "
"%d.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d to %d expected.\n"
msgstr ""
"%s: помилкова кількість вихідних параметрів: слід використовувати значення "
"від %d до %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: помилковий тип вхідного параметра №%d.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: %s has size %d, but %d is expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: %s має розмірність %d, "
"слід було використати розмірність %d.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: %s=%d, but a positive value is "
"expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: %s=%d, але мало бути вказано "
"додатне значення.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: %s=%s, but a floating point integer "
"is expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: %s=%s, але мало бути вказано "
"ціле число з плаваючою крапкою.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: %s=%d, which does not match %s: %d.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: %s=%d, що не відповідає %s: "
"%d.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: %s=%d, which does not match size of "
"%s: %d.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: %s=%d, що не відповідає "
"розмірності %s: %d.\n"

msgid "  Possible matrix is not positive definite."
msgstr "  Можлива матриця не є додатно визначеною."

msgid "Non-positive diagonal encountered, matrix is not positive definite."
msgstr ""
"Виявлено недодатний діагональний елемент. Матриця не є додатно визначеною."

msgid "Insufficient working storage in blkfct, temp(*)."
msgstr "Недостатній робочий обсяг пам’яті у blkfct, temp(*)."

msgid "Insufficient working storage in blkfct, iwork(*)."
msgstr "Недостатній робочий обсяг пам’яті у blkfct, iwork(*)."

msgid " SYMFCT requires PERM to be neqns x 1"
msgstr " Для SYMFCT PERM повинне мати розмірність кількості рівнянь на 1"

msgid " SYMFCT requires INVN to be neqns x 1"
msgstr " Для SYMFCT INVN повинне мати розмірність кількості рівнянь на 1"

msgid " SYMFCT requires CACHSZ  to be 1 x 1"
msgstr " Для SYMFCT CACHSZ повинне мати розмірність 1 на 1"

msgid " Insufficient working storage in sfinit"
msgstr " Недостатній об’єм робочого сховища даних у sfinit"

#, c-format
msgid "Arguments:\n"
msgstr "Параметри:\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Square matrix expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати квадратну "
"матрицю.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A matrix expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути використано матрицю.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Column vector expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип вектора-"
"стовпчика.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: Same size as input argument #%d "
"expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: розмірність має збігатися "
"з розмірністю вхідного параметра №%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Single String expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути вказано один рядок "
"(string).\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: %s, %s, %s or %s expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: мало бути вказано %s, %s, %s "
"або %s.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Scalar expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати скалярний "
"тип.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Column vector expected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: слід використовувати тип "
"вектора-стовпчика.\n"

#, c-format
msgid "%s: Wrong number of input argument: At least %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати не менше за "
"%d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Square matrix expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: слід використовувати "
"квадратну матрицю.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Column vector expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: слід використовувати "
"вектор-стовпчик.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: Scalar expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: слід використовувати "
"число.\n"

#, c-format
msgid "%s: Did not converge.\n"
msgstr "%s: не збігся.\n"

#, c-format
msgid "%s: Wrong number of input arguments: At least %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: мало бути не менше за %d.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d : A real or complex matrix or a sparse "
"matrix or a function expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d : мало бути вказано матрицю з "
"дійсних або комплексних чисел, розріджену матрицю або функцію.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Transpose of the function %s "
"expected.\n"
msgstr ""
"%s: помилкове значення тип вхідного параметра №%d: мала бути функція, "
"транспонована щодо %s.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A transpose of the function %s "
"expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути вказано транспозицію "
"функції %s.\n"

#, c-format
msgid ""
"%s : Calling qmr(A,Ap) is deprecated. Please see qmr documentation for more "
"details.\n"
msgstr ""
"%s: функція qmr(A,Ap) вважається застарілою. Будь ласка, ознайомтеся з "
"документацією щодо qmr, щоб дізнатися більше.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A real or complex, full or sparse "
"column vector expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути вказано дійсний або "
"комплексний, заповнений або розріджений вектор-стовпчик.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A real or complex, full or sparse, "
"square matrix or a function expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути вказано дійсну або "
"комплексну, заповнену або розріджену квадратну матрицю або функцію.\n"

#, c-format
msgid ""
"%s : Calling qmr(...,M1,M1p) is deprecated. Please see qmr documentation for "
"more details.\n"
msgstr ""
"%s: функція qmr(...,M1,M1p) вважається застарілою. Будь ласка, ознайомтеся з "
"документацією щодо qmr, щоб дізнатися більше.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A transpose of the function "
"expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути вказано транспозицію "
"функції.\n"

#, c-format
msgid ""
"%s: Wrong prototype of input argument #%d: If M1 is function, use the header "
"M1(x,t) instead M1(x).\n"
msgstr ""
"%s: помилковий прототип вхідного параметра №%d: якщо M1 є функцією, "
"використовуйте заголовок M1(x,t) замість M1(x).\n"

#, c-format
msgid ""
"%s : Calling qmr(...,M2,M2p) is deprecated. Please see qmr documentation for "
"more details.\n"
msgstr ""
"%s: функція qmr(...,M2,M2p) вважається застарілою. Будь ласка, ознайомтеся з "
"документацією щодо qmr, щоб дізнатися більше.\n"

#, c-format
msgid ""
"%s: Wrong prototype of input argument #%d: If M2 is function, use the header "
"M2(x,t) instead M2(x).\n"
msgstr ""
"%s: помилковий прототип вхідного параметра №%d: якщо M2 є функцією, "
"використовуйте заголовок M2(x,t) замість M2(x).\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A real scalar expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати дійсне "
"число.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d or %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати %d або %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати тип "
"«string».\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: '%s' or '%s' expected.\n"
msgstr ""
"%s: Помилкове значення вхідного параметра №%d: слід використовувати «%s» або "
"«%s».\n"
