#, c-format
msgid "sparse: panic in file `%s' at line %d.\n"
msgstr "sparse: помилка у файлі «%s», рядок %d.\n"

#, c-format
msgid "%s: A zero was encountered on the diagonal the matrix.\n"
msgstr "%s: на діагоналі матриці виявлено нульовий елемент.\n"

#, c-format
msgid "Reordering,  Step = %1d\n"
msgstr "Перевпорядкування, крок = %1d\n"

#, c-format
msgid "Operation count for inner loop of factorization = %d.\n"
msgstr "Кількість операцій для прямого циклу факторизації = %d.\n"

