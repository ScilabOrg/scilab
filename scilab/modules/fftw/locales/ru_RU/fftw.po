#, c-format
msgid "FFTW Library %s not found.\n"
msgstr "Библиотека FFTW %s не найдена.\n"

msgid "FFTW Library not found.\n"
msgstr "Библиотека FFTW не найдена.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d not expected.\n"
msgstr "%s: Неверное количество входных аргументов: Ожидалось %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A Real or Complex expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента #%d: Ожидалось вещественное или "
"комплексное число.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d.\n"
msgstr "%s: Неверные значения входного аргумента #%d.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: N-dimensionnal array expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента #%d: Ожидался N-размерный массив.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s: Неверный размер входного аргумента #%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: int matrix expected.\n"
msgstr ""
"%s: Неверный тип входного аргумента №%d: Ожидалась целочисленная матрица.\n"

#, c-format
msgid ""
"%s: Incompatible input arguments '#%d and '#%d': Same sizes expected.\n"
msgstr ""
"%s: Несовместимые входные аргументы '#%d и '#%d': Они должны иметь "
"одинаковый размер.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Не осталось доступной памяти.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Non-negative integers expected.\n"
msgstr ""
"%s: Неверные значения входного аргумента #%d: Ожидались неотрицательные "
"целые числа.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: FFTW flag expected.\n"
msgstr "%s: Неверные значения входного аргумента #%d: Ожидался флаг FFTW.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d.\n"
msgstr "%s: Неверный тип входного аргумента #%d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A string expected.\n"
msgstr "%s: Неверный тип входного аргумента #%d: Ожидалась строка.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Произошла ошибка: %s\n"

msgid "FFTW can't read wisdom."
msgstr "FFTW не может прочитать файл оптимизации (см. fftw-wisdom)."

msgid "Scilab FFTW module not installed.\n"
msgstr "Модуль Scilab FFTW не установлен.\n"
