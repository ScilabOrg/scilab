<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:svg="http://www.w3.org/2000/svg" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:db="http://docbook.org/ns/docbook" version="5.0-subset Scilab" xml:lang="en" xml:id="atomsInstallRegister">
  <info>
    <pubdate>$LastChangedDate$</pubdate>
  </info>
  <refnamediv>
    <refname>atomsInstallRegister</refname>
    <refpurpose>Add one or more external modules in the list of installed modules</refpurpose>
  </refnamediv>
  <!-- ===================================================================== -->
  <!-- Calling Sequence -->
  <!-- ===================================================================== -->
  <refsynopsisdiv>
    <title>Calling Sequence</title>
    <synopsis>
	  nbAdd = atomsInstallRegister(name,version,status,allusers)
	</synopsis>
  </refsynopsisdiv>
  <!-- ===================================================================== -->
  <!-- Parameters -->
  <!-- ===================================================================== -->
  <refsection>
    <title>Parameters</title>
    <variablelist>
      <varlistentry>
        <term>name</term>
        <listitem>
          <para>
			Matrix of strings : External module's technical name
		  </para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>version</term>
        <listitem>
          <para>
			Matrix of strings : External module's version
		  </para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>status</term>
        <listitem>
          <para>
			Matrix of strings : External module's installation status
		  </para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>allusers</term>
        <listitem>
          <para>
			Matrix of booleans
		  </para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>nbAdd</term>
        <listitem>
          <para>
			
		  </para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>
  <!-- ===================================================================== -->
  <!-- Description -->
  <!-- ===================================================================== -->
  <refsection>
    <title>Description</title>
    <para><literal>atomsInstallRegister</literal> add one or more external modules in the list of installed modules.
	</para>
  </refsection>
  <!-- ===================================================================== -->
  <!-- Authors -->
  <!-- ===================================================================== -->
  <refsection>
    <title>Authors</title>
    <simplelist type="vert">
      <member>Pierre MARECHAL - DIGITEO</member>
    </simplelist>
  </refsection>
</refentry>
