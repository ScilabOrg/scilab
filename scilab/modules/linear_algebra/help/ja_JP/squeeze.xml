      <member>
        <link linkend="hypermat">hypermat</link>
      </member>
      <member>
        <link linkend="hypermatrices">hypermatrices</link>
      </member>
    </simplelist>
  </refsection>
  <refsection>
    <title>作者</title>
    <para>Eric Dubois, Jean-Baptiste Silvy</para>
  </refsection>
</refentry>
