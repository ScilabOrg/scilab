// lsq used with null tolerance now calls backslash operator
rand("seed", 0);
A = rand(4, 2)*rand(2, 3); // A rank 2 matrix
b = rand(4, 1);
X = lsq(A, b, 0);
Rank deficient. rank = 2

refX = A\b;
Rank deficient. rank = 2

assert_checkequal(X, refX);
