// <-- Short Description -->
// We could save a tlist with undefined fields.
newobj = tlist(["MYTYPE"
"a"
"b"
"c" ]);
newobj.a = %t;
newobj.c = 0.05;
saved = newobj;
save("TMPDIR/pb.dat", "newobj");
clear newobj;
load("TMPDIR/pb.dat");
assert_checkequal(newobj, saved);
