#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: сталася помилка: %s\n"

#, c-format
msgid "%s: Incorrect format.\n"
msgstr "%s: помилковий формат.\n"

#, c-format
msgid "%s: Invalid conversion.\n"
msgstr "%s: помилкове перетворення.\n"

#, c-format
msgid "%s: Options ignored.\n"
msgstr "%s: параметри проігноровано.\n"

#, c-format
msgid "%s: Wrong number of input argument(s).\n"
msgstr "%s: помилкова кількість вхідних параметрів.\n"

#, c-format
msgid "%s: Wrong number of output argument(s).\n"
msgstr "%s: помилкова кількість вихідних параметрів.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A String expected.\n"
msgstr ""
"%s: помилковий розмір вхідного параметра №%d: слід використовувати рядок.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A file descriptor expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: слід використовувати "
"дескриптор файла.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A filename expected.\n"
msgstr ""
"%s: помилковий розмірність вхідного параметра №%d: слід використовувати "
"назву файла.\n"

#, c-format
