msgstr "нижняя частота отсечки (0.<fниж<.5):"

msgid "high cut-off frequency (0.<flow<fhi<.5):"
msgstr "верхняя частота отсечки (0.<fниж<fверх<.5):"

msgid "Incorrect low cut-off frequency"
msgstr "Неправильная нижняя частота отсечки"

msgid "Incorrect high cut-off frequency"
msgstr "Неправильная верхняя частота отсечки"

msgid "Even length high pass and stop band filters not allowed"
msgstr ""
"Чётные длины фильтра нижних частот и режекторного фильтра не разрешены"

msgid "---Filter order is being incremented by 1"
msgstr "---Порядок фильтра увеличивается на 1"

msgid "Kaiser"
msgstr "Кайзера"

msgid "Chebyshev"
msgstr "Чебышева"

msgid "Rectangular"
msgstr "Прямоугольное"

msgid "Triangular"
msgstr "Треугольное"

msgid "Hamming"
msgstr "Хэмминга"

msgid "Input window type"
msgstr "Тип окна на входе"

msgid "Input window characteristics"
msgstr "Характеристики окна на входе"

msgid "Incorrect beta value"
msgstr "Неправильное значение беты"

msgid "Input window characteristics:"
msgstr "Характеристики окна на входе:"

msgid "dp (dp>0)   : the maximum value of the window side-lobe height"
msgstr "dp (dp>0)   : максимальное значение высоты боковых лепестков окна"

msgid "df (0<df<.5): the width of the window main lobe"
msgstr "df (0<df<.5): ширина главного лепестка окна"

msgid "only one of this two values is to be defined,"
msgstr "только одно из этих двух значений нужно определить"

msgid "the other one is automatically deduced"
msgstr "другое значение получается автоматически"

msgid "name of specified value"
msgstr "имя установленного значения"

msgid "value"
msgstr "значение"

msgid "Incorrect name of specified value"
msgstr "Неверное имя установленного значения"

msgid "Incorrect  value"
msgstr "Неверное значение"

#, c-format
msgid "%s: Input argument #%d must be strictly positive.\n"
msgstr "%s: Входной аргумент №%d должен быть строго положительным.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: incorrect element #%d\n"
msgstr ""
"%s: Неверное значение входного аргумента №%d: некорректный элемент №%d\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: The first element must be %s and "
