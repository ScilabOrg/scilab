#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr "%s: Неверный тип аргумента #%d: Ожидалось вещественное число.\n"

#, c-format
msgid "%s: An error occurred: %s\n"
msgstr "%s: Произошла ошибка: %s\n"

#, c-format
msgid "%s: Wrong values for input argument #%d"
msgstr "%s: Неверные значения аргумента #%d"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Must be in the interval [%d, %d].\n"
msgstr ""
"%s: Неверное значение аргумента #%d: Должно быть в интервале [%d, %d].\n"

#, c-format
msgid "%s: Incompatible input arguments #%d and #%d: Same sizes expected.\n"
msgstr ""
"%s: Несовместимые аргументы #%d и #%d: Они должны иметь одинаковые размеры.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Must be in the interval [%s, %s].\n"
msgstr ""
"%s: Неверное значение аргумента #%d: Должно быть в интервале [%s, %s].\n"

#, c-format
msgid "%s: Wrong number of input argument(s).\n"
msgstr "%s: Неверное количество аргументов.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s: Неверный размер аргумента #%d.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: The first frequency must be 0 and "
"the last 1.\n"
msgstr ""
"%s: Неверные значения аргумента #%d: Первая частота должна быть 0 а "
"последняя 1.\n"

msgid "Unknown design type --- program termination."
msgstr "Неизвестный тип модели --- программа завершается."

#, c-format
msgid "%s: Wrong value for input argument #%d: %s\n"
msgstr "%s: Неверное значение аргумента #%d: %s\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be between %d and %d.\n"
msgstr "%s: Неверное значение аргумента #%d: Должно быть между %d и %d.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr "%s: Неверное количество аргументов: Ожидалось от %d до %d.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: N-dimensionnal array expected.\n"
msgstr "%s: Неверный тип аргумента #%d: Ожидался N-размерный массив.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A string expected.\n"
msgstr "%s: Неверный тип аргумента #%d: Ожидалась строка.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be less than 0.5\n"
msgstr "%s: Неверное значение аргумента #%d: Должно быть менее 0.5\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be [%d %d].\n"
msgstr "%s: Неверное значение аргумента #%d: Должно быть [%d %d].\n"

#, c-format
msgid "%s: Wrong type for input argument(s).\n"
msgstr "%s: Неверны типы аргументов.\n"

#, c-format
msgid "%s: Input argument #%d must be equal to 1 or 2.\n"
msgstr "%s: Аргумент #%d должен равняться 1 или 2.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s: Неверное количество аргументов: Ожидалось %d.\n"
