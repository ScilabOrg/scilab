#, c-format
msgid "%s: Arguments #%d and #%d must have the same sizes.\n"
msgstr ""
"%s: Les arguments n° %d et n° %d doivent avoir les mêmes dimensions.\n"

#, c-format
msgid ""
"%s: Incompatible input arguments #%d and #%d: Argument #%d expected to be "
"less than argument #%d.\n"
msgstr ""
"%s: Les arguments d'entrée n°%d et n°%d sont incompatibles: L'argument  n°%d "
"doit être plus petit que l'argument n°%d.\n"

#, c-format
msgid "%s: Incompatible input arguments #%d and #%d: Same degree expected.\n"
msgstr ""
"%s: Les arguments d'entrée n°%d et n°%d sont incompatibles: Ils doivent "
"avoir le même degré.\n"

#, c-format
msgid ""
"%s: Incompatible input arguments #%d and #%d: Same numbers of elements "
"expected.\n"
msgstr ""
"%s : Arguments d'entrée n°%d et n°%d incompatibles : Même nombre d'éléments "
"attendu.\n"

#, c-format
msgid "%s: Incompatible input arguments #%d and #%d: Same sizes expected.\n"
msgstr ""
"%s : Arguments d'entrée n°%d et n°%d incompatibles : Même dimension "
"attendue.\n"

#, c-format
msgid "%s: Input argument #%d must be real.\n"
msgstr "%s : L'argument d'entrée n°%d doit être réel.\n"

#, c-format
msgid "%s: Too abrupt change near end of frequency range.\n"
msgstr ""
"%s: Changement trop brusque au voisinage de la fin de la bande de "
"fréquence.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s : Mauvais nombre d'argument(s) en entrée : %d attendu.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d or %d expected.\n"
msgstr "%s : Mauvais nombre d'arguments d'entrée : %d ou %d attendus.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d to %d expected.\n"
msgstr "%s : Mauvais nombre d'arguments d'entrée : %d à %d attendus.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A %d elements array expected.\n"
msgstr ""
"%s: Mauvaise dimension pour l'argument d'entrée n°%d: Un tableau de %d "
"éléments est attendu.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A polynomial expected.\n"
msgstr ""
"%s : Mauvaise taille pour l'argument d'entrée n°%d : Un polynôme est "
"attendu.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A scalar expected.\n"
msgstr ""
"%s : Mauvaise dimension pour l'argument d'entrée n°%d : Un scalaire "
"attendu.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d: A single input, single output system "
"expected.\n"
msgstr ""
"%s: Mauvaise taille pour l'argument d'entrée n° %d: Un système mono-entrée, "
"mono-sortie est attendu.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: A tall matrix expected.\n"
msgstr ""
"%s: Mauvaise dimensions pour l'argument d'entrée n°%d: Le nombre de lignes "
"doit être >= au nombre de colonnes.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d:  Linear dynamical system or row "
"vector of floats expected.\n"
msgstr ""
"%s: Mauvais type pour l'argument d'entrée n°%d: Un système dynamique "
"linéaire ou un vecteur de flottants est attendu.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Array of floating point numbers "
"expected.\n"
msgstr ""
"%s : Mauvais type pour l'argument d'entrée n°%d : un tableau de nombres "
"flottants est attendu.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: rational fraction array expected.\n"
msgstr ""
"%s : Mauvais type pour l'argument d'entrée n°%d : tableau de fraction "
"rationnelle attendu.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Must be in the interval [%s, %s].\n"
msgstr ""
"%s : Mauvaise valeur pour l'argument d'entrée n° %d : Doit être dans "
"l'intervalle [%s, %s].\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: Must be in the set {%s}.\n"
msgstr ""
"%s : Mauvaise valeur pour l'argument d'entrée n°%d : Doit être dans "
"l'ensemble {%s}.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Distance between Max and Min "
"elements must be greater than %g.\n"
msgstr ""
"%s: Mauvaise valeur  pour l'argument d'entrée n°%d: La distance entre le "
"plus grand et le plus petit élement doit être plus grande que %g.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Elements must be in increasing "
"order.\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée n°%d: Les élements doivent être "
"en ordre croissant.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Elements must be in the interval "
"[%s, %s].\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée n°%d: Les élements doivent être "
"dans l'intervalle [%s, %s].\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Elements must be positive.\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée n°%d: Les élements doivent être "
"positifs.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: Non-negative integers expected.\n"
msgstr ""
"%s : Mauvaise valeurs pour l'argument d'entrée n°%d : Un entier non-négatif "
"attendu.\n"

#, c-format
msgid "%s: Wrong values for input argument #%d: Not sampled high enough.\n"
msgstr ""
"%s: Mauvaise valeur  pour l'argument d'entrée n°%d: L'échantillonage n'est "
"pas assez fin.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: The first element must be %s and "
"the last %s.\n"
msgstr ""
"%s: Mauvaise valeur  pour l'argument d'entrée n°%d: Le premier élement doit "
"être  %s et le dernier %s.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d: degree must be in the set {%s}.\n"
msgstr ""
"%s: Mauvaise valeur pour l'argument d'entrée n°%d: Le degré doit être dans "
"l'ensemble {%s}.\n"

msgid "Arma identifcation"
msgstr "Identification ARMA"

msgid "Signal Processing"
msgstr "Traitement du signal"

msgid "Spectral power"
msgstr "Puissance spectrale"

msgid "Theoritical value"
msgstr "Valeur théorique"

msgid "Using macro mese"
msgstr "Utilise la macro mese"

msgid "estimates xf"
msgstr "Estime xf"

msgid "estimates xs"
msgstr "Estime xs"

msgid "frequency"
msgstr "fréquence"

msgid "real state"
msgstr "état réel"

msgid "spectral estimate"
msgstr "estimation spectrale"
