install-libm2sci_la_etcDATA: $(libm2sci_la_etc_DATA)
	@$(NORMAL_INSTALL)
	@list='$(libm2sci_la_etc_DATA)'; test -n "$(libm2sci_la_etcdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(libm2sci_la_etcdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(libm2sci_la_etcdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(libm2sci_la_etcdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(libm2sci_la_etcdir)" || exit $$?; \
	done

