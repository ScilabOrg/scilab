msgid "Metanet"
msgstr "Metanet"

msgid "&File"
msgstr "&Файл"

msgid "&Tools"
msgstr "&Сервис"

msgid "&Edit"
msgstr "&Правка"

msgid "&?"
msgstr "Справка"

#, c-format
msgid "%s: First argument must be a vector of size greater or equal to 2.\n"
msgstr ""
"%s: Первый аргумент должен быть вектором, размером более или равным 2.\n"

#, c-format
msgid ""
"%s: Second argument must be a vector with same dimension as the first one.\n"
msgstr ""
"%s: Второй аргумент должен быть вектором с размерностью, как у первого.\n"

#, c-format
msgid "%s: Third argument must be a vector of size greater or equal to 1.\n"
msgstr ""
"%s: Третий аргумент должен быть вектором, размером более или равным 1.\n"

#, c-format
msgid "%s: First argument must be a vector with positive integer values.\n"
msgstr ""
"%s: Первый аргумент должен быть вектором с положительными целыми "
"значениями.\n"

#, c-format
msgid "%s: Second argument must be a vector with positive integer values.\n"
msgstr ""
"%s: Второй аргумент должен быть вектором с положительными целыми "
"значениями.\n"

#, c-format
msgid "%s: Third argument must be a vector with positive integer values.\n"
msgstr ""
"%s: Третий аргумент должен быть вектором с положительными целыми "
"значениями.\n"

msgid "Running out of memory"
msgstr "Выход за пределы памяти"

#, c-format
msgid "\"%s\" is a directory"
msgstr "\"%s\" является каталогом"

#, c-format
msgid "Directory \"%s\" does not exist"
msgstr "Каталог \"%s\" не существует"

#, c-format
msgid "Graph file \"%s/%s.graph\" does not exist"
msgstr "Файл графа \"%s/%s.graph\" не существует"

#, c-format
msgid "Unable to open file \"%s/%s.graph\""
msgstr "Невозможно открыть файл \"%s/%s.graph\""

#, c-format
msgid "Bad graph file. Node \"%s\" is duplicated"
msgstr "Повреждённый файл графа. Узел \"%s\" дублирован"

#, c-format
msgid "Bad graph file. Node \"%s\" referenced by arc \"%s\" not found"
msgstr ""
"Повреждённый файл графа. Узел \"%s\" полученный по дуге \"%s\" не найден"

msgid "Scilab module metanet not installed.\n"
msgstr "Модуль Scilab metanet не установлен.\n"

#, c-format
msgid "Bad internal node number %d"
msgstr "Неверный внутренний номер узла %d"

#, c-format
msgid "Bad internal arc number %d"
msgstr "Неверный внутренний номер дуги %d"

#, c-format
msgid "Unable to write file in directory %s, check access"
msgstr "Невозможно записать файл в каталог %s, проверьте права доступа"
