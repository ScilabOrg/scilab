msgid "Metanet"
msgstr "Metanet"

msgid "&File"
msgstr "&Datei"

msgid "&Tools"
msgstr "&Zusatzprogramme"

msgid "&Edit"
msgstr "&Editieren"

msgid "&?"
msgstr "&?"

#, c-format
msgid "%s: First argument must be a vector of size greater or equal to 2.\n"
msgstr "%s: erstes Argument muß ein Vektor mindestens der Größe 2 sein.\n"

#, c-format
msgid ""
"%s: Second argument must be a vector with same dimension as the first one.\n"
msgstr ""
"%s: zweites Argument muß ein Vektor mit der gleichen Dimension wie der Erste "
"sein.\n"

#, c-format
msgid "%s: Third argument must be a vector of size greater or equal to 1.\n"
msgstr "%s: drittes Argument muß ein Vektor mindestens der Größe 1 sein.\n"

#, c-format
msgid "%s: First argument must be a vector with positive integer values.\n"
msgstr ""
"%s: erstes Argument muß ein Vektor mit positiven Integerwerten sein.\n"

#, c-format
msgid "%s: Second argument must be a vector with positive integer values.\n"
msgstr ""
"%s: zweites Argument muß ein Vektor mit positiven Integerwerten sein.\n"

#, c-format
msgid "%s: Third argument must be a vector with positive integer values.\n"
msgstr ""
"%s: drittes Argument muß ein Vektor mit positiven Integerwerten sein.\n"

msgid "Running out of memory"
msgstr "Speicher wird voll"

#, c-format
msgid "\"%s\" is a directory"
msgstr "'%s' ist ein Verzeichnis"

#, c-format
msgid "Directory \"%s\" does not exist"
msgstr "Verzeichnis '%s' existiert nicht"

#, c-format
msgid "Graph file \"%s/%s.graph\" does not exist"
msgstr "Graphendatei '%s/%s.graph' existiert nicht"

#, c-format
msgid "Unable to open file \"%s/%s.graph\""
msgstr "kann Datei '%s/%s.graph' nicht öffnen"

#, c-format
msgid "Bad graph file. Node \"%s\" is duplicated"
msgstr "Fehlerhafte Graphendatei. Knoten '%s' ist dupliziert."

#, c-format
msgid "Bad graph file. Node \"%s\" referenced by arc \"%s\" not found"
msgstr ""
"Fehlerhafte Graphendatei. Knoten '%s' referenziert durch arc '%s' nicht "
"gefunden."

msgid "Scilab module metanet not installed.\n"
msgstr "Scilab Modul Metanet nicht installiert.\n"

#, c-format
msgid "Bad internal node number %d"
msgstr "Fehlerhafte interne Knotennummer %d"

#, c-format
msgid "Bad internal arc number %d"
msgstr "Fehlerhafte arc Nummer %d"

#, c-format
msgid "Unable to write file in directory %s, check access"
msgstr "Kann in das Verzeichnis %s nicht schreiben, überprüfe die Adresse"
