msgid "Metanet"
msgstr "Metanet"

msgid "&File"
msgstr "&Fitxer"

msgid "&Tools"
msgstr "&Eines"

msgid "&Edit"
msgstr "&Edita"

msgid "&?"
msgstr "&?"

#, c-format
msgid "%s: First argument must be a vector of size greater or equal to 2.\n"
msgstr ""
"%s: El primer argument ha de ser un vector de mida major o igual que 2.\n"

#, c-format
msgid ""
"%s: Second argument must be a vector with same dimension as the first one.\n"
msgstr ""
"%s: El segon argument ha de ser un vector de la mateixa dimensió que el "
"primer.\n"

#, c-format
msgid "%s: Third argument must be a vector of size greater or equal to 1.\n"
msgstr ""
"%s: El tercer argument ha de ser un vector de mida major o igual que 1.\n"

#, c-format
msgid "%s: First argument must be a vector with positive integer values.\n"
msgstr ""
"%s: El primer argument ha de ser un vector amb valors enters positius.\n"

#, c-format
msgid "%s: Second argument must be a vector with positive integer values.\n"
msgstr ""
"%s: El segon argument ha de ser un vector amb valors enters positius.\n"

#, c-format
msgid "%s: Third argument must be a vector with positive integer values.\n"
msgstr ""
"%s: El tercer argument ha de ser un vector amb valors enters positius.\n"

msgid "Running out of memory"
msgstr "No hi ha més memòria"

#, c-format
msgid "\"%s\" is a directory"
msgstr "«%s» és un directori"

#, c-format
msgid "Directory \"%s\" does not exist"
msgstr "El directori \"%s\" no existeix"

#, c-format
msgid "Graph file \"%s/%s.graph\" does not exist"
msgstr "El fitxer gràfic \"%s/%s.graph\" no existeix"

#, c-format
msgid "Unable to open file \"%s/%s.graph\""
msgstr "No s'ha pogut obrir el fitxer \"%s/%s.graph\""

#, c-format
msgid "Bad graph file. Node \"%s\" is duplicated"
msgstr "El fitxer gràfic és incorrecte. Hi ha duplicat el node \"%s\""

#, c-format
msgid "Bad graph file. Node \"%s\" referenced by arc \"%s\" not found"
msgstr ""
"El fitxer gràfic és incorrecte. No s'ha trobat el node \"%s\" referenciat "
"per l'arc \"%s\""

msgid "Scilab module metanet not installed.\n"
msgstr "No està instal·lat el mòdul metanet del Scilab.\n"

#, c-format
msgid "Bad internal node number %d"
msgstr "El número intern de node %d és incorrecte"

#, c-format
msgid "Bad internal arc number %d"
msgstr "El número intern d'arc %d és incorrecte"

#, c-format
msgid "Unable to write file in directory %s, check access"
msgstr "No s'ha pogut escriure el fitxer al directori %s, comproveu l'accés"
