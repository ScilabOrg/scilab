mode(-1)

while %t do
  num=tk_choose(["Spherical pendulum";"Sliding pendulum"],..
      "Examples of DAE");
  deff("[]=demoexc(fil)","exec(""SCI/demos/simulation/dae/""+fil)")
      
  select num
  case 0
    return
  case 1
    demoexc("dae1.dem");
  case 2
    demoexc("dae2.dem");
  end
end
mode(0)
