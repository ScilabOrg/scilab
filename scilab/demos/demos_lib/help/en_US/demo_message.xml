<!DOCTYPE MAN SYSTEM "../../../../modules/helptools/help.dtd">
<MAN>
	<LANGUAGE>eng</LANGUAGE>
	<TITLE>demo_message</TITLE>
	<TYPE>Scilab Function</TYPE>
    <DATE>$LastChangedDate$</DATE>
	
	<SHORT_DESCRIPTION name="demo_message"> display a message </SHORT_DESCRIPTION>
	
	<CALLING_SEQUENCE>
		<CALLING_SEQUENCE_ITEM>demo_message(fil)</CALLING_SEQUENCE_ITEM>
	</CALLING_SEQUENCE>
	
	<DESCRIPTION>
		<P>
		The function demo_message displays the text message within the file 'fil' given 
		in argument.
		</P>
	</DESCRIPTION>
	
	<EXAMPLE><![CDATA[
demo_message('SCI/demos/control/pid/pid_3.sce');
	]]></EXAMPLE>
	
