    <load on="pdf_ps_eps_graphic_export"/>
    <load on="Xcos"/>
</path>

<!-- PDF doc generation (dep of jeuclid) -->
<path value="@XMLGRAPHICS_COMMONS@" load="onUse">
    <load on="documentationGeneration"/>
    <load on="graphics_mathml_textrendering"/>
    <load on="pdf_ps_eps_graphic_export"/>
